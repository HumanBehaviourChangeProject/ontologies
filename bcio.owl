        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A teaching professional that promotes the social, physical, and intellectual development of children below primary school age through the provision of educational and play activities.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 2342</rdfs:isDefinedBy>
        <rdfs:label>early childhood educator</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010031 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010031">
