            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An individual human behaviour that is reflectively driven.</obo:IAO_0000115>
        <rdfs:label>reflective behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050833 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050833">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050435"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A behavioural attribute in which the behaviour is caused by a reflective mental process.</obo:IAO_0000115>
        <rdfs:label>reflectively driven</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050834 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050834">
