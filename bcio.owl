        <rdfs:label>deductive reasoning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050626 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050626">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000193"/>
