        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A teaching professional that teaches or instructs vocational or occupational subjects in adult and further education institutions and to senior students in secondary schools and colleges.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 232</rdfs:isDefinedBy>
        <rdfs:label>vocational education teacher</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010028 -->

