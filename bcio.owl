        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A teaching professional that teaches teach a range of subjects at the primary education level.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 2341</rdfs:isDefinedBy>
        <rdfs:label>primary school teacher</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010030 -->

