        <rdfs:label>egoistic personal value</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050638 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050638">
