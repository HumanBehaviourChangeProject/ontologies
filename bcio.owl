        <rdfs:label>comparative desirability appraisal</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050618 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050618">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000006"/>
        <obo:IAO_0000112>&quot;Reassessment and redefinition of the situation&quot; in the Norm Activation Theory</obo:IAO_0000112>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
