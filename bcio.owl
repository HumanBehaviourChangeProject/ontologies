        <rdfs:isDefinedBy>ISCO: 26</rdfs:isDefinedBy>
        <rdfs:label>legal, social and cultural professional</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010035 -->

