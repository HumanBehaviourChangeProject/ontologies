        <rdfs:isDefinedBy>ISCO: 233</rdfs:isDefinedBy>
        <rdfs:label>secondary education teacher</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010029 -->

