        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A teaching professional that teaches children, young persons or adults with physical or intellectual special needs.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 2352</rdfs:isDefinedBy>
        <rdfs:label>special needs teacher</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010032 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010032">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010025"/>
        <obo:IAO_0000112>Guitar teacher (private tuition), Piano teacher (private tuition), Singing teacher (private tuition), Violin teacher (private tuition)</obo:IAO_0000112>
