        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about how serious the harm associated with an outcome could be.</obo:IAO_0000115>
        <obo:IAO_0000118>belief about threat severity</obo:IAO_0000118>
        <rdfs:label>belief about severity of an outcome</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006031 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006031">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006305"/>
