    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050822">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050435"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A behavioural attribute that is the extent to which one’s behaviour is believed to be consistent with one’s core, positive self-identity.</obo:IAO_0000115>
        <rdfs:label>identity congruence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050823 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050823">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050806"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A behavioural consequence that involves an outcome relating to the environment system.</obo:IAO_0000115>
        <rdfs:label>impact of behaviour on environment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050824 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050824">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050291"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A knowledge development behaviour where the knowledge is about how to reduce or avoid a harmful behaviour.</obo:IAO_0000115>
        <rdfs:label>knowledge acquisition about reducing harmful behaviours</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050825 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050825">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050291"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A knowledge development behaviour that involves gathering information over a period of time.</obo:IAO_0000115>
        <rdfs:comment>This is often used in relation to regulatory behaviour, either oneself or others. &apos;Monitoring behaviour&apos; has part &apos;regulatory behaviour&apos;</rdfs:comment>
        <rdfs:label>monitoring behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050826 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050826">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036086"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>Nurture behaviour that is provided to a young person by someone in a parent role.</obo:IAO_0000115>
        <rdfs:label>parenting behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050827 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050827">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036000"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An individual human behaviour that is the target of an intervention and has been previously enacted.</obo:IAO_0000115>
        <rdfs:comment>This is a relational class (relates one behaviour to another).</rdfs:comment>
        <rdfs:label>past occurrence of a target behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050828 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050828">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050806"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A behavioural consequence that is positively evaluated by an individual or a population.</obo:IAO_0000115>
        <rdfs:label>positive behavioural consequence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050829 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050829">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036035"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An economic behaviour in which the person gives money in exchange for a service or ownership of a product.</obo:IAO_0000115>
        <rdfs:label>purchasing behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050830 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050830">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A process in which physical or mental uneasiness or distress is reduced.</obo:IAO_0000115>
        <rdfs:label>reducing discomfort</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050831 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050831">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A process in which damage or injury is reduced.</obo:IAO_0000115>
        <rdfs:label>reducing harm</rdfs:label>
    </owl:Class>
    


