        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive representation of one’s behaviour&gt; that the person could enact instead of one that they have enacted or are enacting.</obo:IAO_0000115>
        <rdfs:label>cognitive representation of an alternative behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050615 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050615">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000112>“Immediate competing demands and preferences” in the Health Promotion Model</obo:IAO_0000112>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive representation&gt; of a behaviour that the person is enacting or has enacted.</obo:IAO_0000115>
        <rdfs:label>cognitive representation of one’s behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050616 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050616">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006012"/>
