        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">agree behaviour goal BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007005 -->

