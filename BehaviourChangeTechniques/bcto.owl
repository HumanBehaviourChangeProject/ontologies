    <!-- http://purl.obolibrary.org/obo/OMRSE_00000222 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMRSE_00000222">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000033"/>
        <obo:IAO_0000111 xml:lang="en">debt obligation</obo:IAO_0000111>
        <obo:IAO_0000112 xml:lang="en">Three Arrows Capital declared bankruptcy on July 1, 2022 after defaulting on a $665 million debt obligation owed to Voyager Digital.</obo:IAO_0000112>
