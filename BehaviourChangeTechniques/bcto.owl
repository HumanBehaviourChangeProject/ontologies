        <obo:IAO_0000119 rdf:resource="http://purl.obolibrary.org/obo/ro/docs/causal-relations"/>
        <obo:IAO_0000589>causally influenced by (entity-centric)</obo:IAO_0000589>
        <rdfs:label>causally influenced by</rdfs:label>
    </owl:ObjectProperty>
    


