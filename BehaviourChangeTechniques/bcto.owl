        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007001"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;goal directed BCT&gt; that advises the person to plan a way of performing the behaviour that is pleasurable or satisfying.</obo:IAO_0000115>
        <rdfs:label>plan inclusion of enjoyment BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007062 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007062">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_033000"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;behaviour change technique&gt; that draws attention to consequences of the behaviour in the normal course of events.</obo:IAO_0000115>
        <rdfs:label>increase awareness of consequences BCT</rdfs:label>
    </owl:Class>
    


