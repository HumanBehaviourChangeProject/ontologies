        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An increase awareness of consequences BCT that guides the person to imagine and compare the consequences of performing and not performing the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">prompt comparative imagining of future outcomes BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007072 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007072">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007162"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is a broad category. It can include both advising people on ways to change emotions and directly intervening to change emotions.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behaviour change technique that increases awareness of what other people think, do, or feel.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">awareness of other peoples thoughts, feelings and actions BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007073 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007073">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007072"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An awareness of other people&apos;s thoughts, feelings and actions BCT that draws attention to other people&apos;s behaviour and compares it with the person&apos;s own behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">prompt social comparison BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007074 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007074">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007072"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An awareness of other people&apos;s thoughts, feelings and actions BCT that increases awareness of whether others will like, approve, dislike, or disapprove of the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">increase awareness of others approval BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007075 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007075">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007072"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An awareness of other people&apos;s thoughts, feelings and actions BCT that presents information from a credible person or organisation to influence the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">present information from credible influence BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007076 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007076">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007072"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A proxy interpersonal process is where the person interacts with a chatbot, digital assistant, or similar computer program designed to simulate and process human communication</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An awareness of other people&apos;s thoughts, feelings and actions BCT that involves sharing a view about what behaviour the person should do.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">suggest to change behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007077 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007077">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007072"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To qualify as negative, the consequence should be negatively valued by the person receiving it.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An awareness of other people&apos;s thoughts, feelings and actions BCT that involves telling the person to perform the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tell to change behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007078 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007078">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007072"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An awareness of other people&apos;s thoughts, feelings and actions BCT that makes an emotional or earnest appeal to change the person&apos;s behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">implore to change behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007079 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007079">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007090"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An associative learning BCT that involves creating, strengthening or reducing an association between the behaviour and an environmental trigger.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">alter external stimulus BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007080 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007080">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007079"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An alter external stimulus BCT that involves introducing an external stimulus to facilitate the behaviour for which an intention has previously been formed.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">prompt intended action BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007081 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007081">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007079"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An alter external stimulus BCT that introduces external information that is already associated with the behaviour in order to elicit that behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cue BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007082 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007082">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007079"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Example 1: Changing meal portion size to reduce the amount people eat at a sitting.
Example 2: Removing unhealthy snacks from the till area in supermarkets to reduce impulse buying of these items.
Example 3: Organisationwide use of red time notices that people can put on their door or desk to signal that they are not to be interrupted in order to increase productivity.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An alter external stimulus BCT that identifies an external stimulus that reliably signals that reward will follow the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cue signalling reward BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007084 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007084">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007079"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An alter external stimulus BCT in which cues for the behaviour are presented less frequently.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">reduce cue frequency BCTs</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007085 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007085">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007079"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An alter external stimulus BCT that sustains the person&apos;s exposure to a stimulus associated with the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">expose to stimulus BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007087 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007087">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007085"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An expose to stimulus BCT that provides sustained exposure to a stimulus to reduce or extinguish an unwanted behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">arrange satiation BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007089 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007089">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007085"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An expose to stimulus BCT that gradually exposes the person to an aversive stimulus while also engaging them in a relaxation process to reduce the response to that stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">systematically desensitise BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007090 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007090">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007162"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behaviour change technique that involves repeated pairing of a stimulus with another stimulus or with a behavioural outcome.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associative learning BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007091 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007091">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007090"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An associative learning BCT that involves repeated pairing of a neutral stimulus with a stimulus that already elicits the behaviour until the neutral stimulus elicits the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associative learning to elicit behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007092 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007092">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007090"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a pedometer to keep track of number of steps taken in a day.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An associative learning BCT that stops the presentation of a reward for the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associative learning to extinguish behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007094 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007094">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007168"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An advise specific behaviour BCT that advises repetition of the behaviour in a way that has the function of increasing the skill in performing the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">practise behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007095 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007095">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007168"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An advise specific behaviour BCT that advises the person to replace the unwanted behaviour with another behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">substitute behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007096 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007096">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007168"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An advise specific behaviour BCT that advises the person to repeat the behaviour in the same context.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">context-specific repetition of behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007097 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007097">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007168"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An advise specific behaviour BCT that advises the person to repeat an alternative behaviour consistently in a context that previously elicited an unwanted behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">context-specific repetition of alternative behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007098 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007098">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007168"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An advise specific behaviour BCT that advises the person to repeat a wanted behaviour in an exaggerated way following performance of an unwanted behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">overcorrect unwanted behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007099 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007099">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007168"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An advise specific behaviour BCT that advises the person to perform the behaviour which is already performed in a particular context, in a similar context.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">generalise behaviour BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007100 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007100">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007001"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To qualify as positive, the consequence should have been positively valued by the person receiving it.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A goal directed BCT that sets easy-to-perform tasks for the person, making them increasingly difficult, but achievable, until the behaviour is performed.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">set graded tasks BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007101 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007101">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007162"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The target of the mental process advised by this BCT will be an emotion. If a BCT suggests adopting a different cognitive perspective on behaviour, which then changes emotions, use &quot;suggest different perspective on behaviour BCT&quot; or one of its child classes.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behaviour change technique that alters the consequences or promised consequences for the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural consequence BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007118 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007118">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007062"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">For the purposes of this BCT, preceding behaviour can be considered part of the context</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An increase awareness of consequences BCT that guides the person to imagine performing the unwanted behaviour in a real-life situation followed by experiencing an unpleasant consequence for performing that behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">imagine punishment BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007119 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007119">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007062"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An increase awareness of consequences BCT that guides the person to imagine performing the wanted behaviour in a real-life situation followed by experiencing a pleasant consequence for performing that behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">imagine reward BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007120 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007120">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007062"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An increase awareness of consequences BCT that prompts observation of another person being rewarded when they perform the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">vicarious reward BCT</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_007121 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_007121">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_007062"/>
        <obo:IAO_0000078 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published</obo:IAO_0000078>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A proxy interpersonal process is where the person interacts with a chatbot, digital assistant, or similar computer program designed to simulate and process human communication</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An increase awareness of consequences BCT that prompts observation of another person being punished when they perform the behaviour.</obo:IAO_0000115>
