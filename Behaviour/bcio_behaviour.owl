        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A goal-oriented behaviour that involves making a distinction in favor of or against a person or thing on the basis of the group, class, or category to which the person or thing belongs.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dictionary.com</obo:IAO_0000119>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">discrimination behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_036102 -->

