        <rdfs:label>belief about task complexity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050602 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050602">
