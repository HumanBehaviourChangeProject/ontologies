        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;conditioned reinforcer&gt; that has acquired its reinforcing property by having been associated with a positive reinforcer.</obo:IAO_0000115>
        <rdfs:label>conditioned positive reinforcer</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050621 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050621">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050756"/>
        <obo:IAO_0000112>&quot;Conditioned reinforcer&quot; in the Operant Conditioning</obo:IAO_0000112>
