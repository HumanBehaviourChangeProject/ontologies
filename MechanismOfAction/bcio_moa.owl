    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050573 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050573">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006015"/>
