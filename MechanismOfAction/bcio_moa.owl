        <rdfs:label>cognitive representation of an alternative behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050615 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050615">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
