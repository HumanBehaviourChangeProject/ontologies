    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050617 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050617">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000112>&quot;Desirability&quot; in the Action Theory Model of Consumption</obo:IAO_0000112>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
