        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A disposition that inheres in a person.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">personal disposition</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050302 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050302">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_043000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A process attribute that is an attribute of a life process.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">personal life attribute</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050303 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050303">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050308"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A quality that inheres in a person.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">personal quality</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050304 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050304">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000195"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An affective process involving sensory experience of physiological stimuli.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bodily feeling</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050305 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050305">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050308"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A quality that inheres in some extended organism.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bodily quality</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050306 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050306">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An independent continuant that is spatially extended whose identity is independent of that of other entities and can be maintained through time.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">material entity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050307 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050307">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050305"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily quality that inheres in those structures of the extended organism that are essential for mental functioning.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental quality</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050308 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050308">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A dependent entity that inheres in a bearer by virtue of how the bearer is related to other entities.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">quality</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050310 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050310">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A dependent continuant which is about a portion of reality.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">representation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/ENVO_01000254 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ENVO_01000254">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/RO_0002577"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;The term &apos;disposition&apos; is used to describe that an environmental system can surround and interact with one or more material entities. This conditionality is important, as an entity does not necessarily have to be in an specific environment system at all times.

The class &apos;environmental system&apos; relates to what is found in the physical and social environment , while the class &apos;opportunity&apos; is about the features of the environment and how they interact with the person .

&apos;environmental system&apos; includes physical and social aspects of the environment. As all subclasses of &apos;environmental system&apos; will have physical aspects, a separate subclass for &apos;physical environmental system&apos; would not capture a unique aspect of its parent class.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A system which has the disposition to surround and interact with one or more material entities.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">environmental system</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/ENVO_01000452 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ENVO_01000452">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Mental disposition&apos; is a subclass of &apos;bodily disposition&apos;, as mental dispositions occur in the body.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A disposition which is realised by an environmental system or system parts thereof.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">environmental disposition</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/ENVO_01001110 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ENVO_01001110">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000254"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An environmental system which includes both living and non-living components.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ecosystem</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0007613 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0007613">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The activities involved in the mental information processing system that receives , modifies, stores, and retrieves informational stimuli.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">memory</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0046958 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0046958">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050239"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A simple form of learning whereby the repeated presence of a stimulus leads to a change in the probability or strength of the response to that stimulus. There is no association of one type of stimulus with another, rather it is a generalized response to the environment.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">non-associative learning</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0046959 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0046959">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0046958"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A decrease in a behavioural response to a repeated stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">habituation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0046960 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0046960">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0046958"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An increase in a behavioural response to a repeated stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sensitization</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0098596 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0098596">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0098597"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Observational learning in which new behaviours are acquired through imitation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">imitative learning</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0098597 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0098597">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050239"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Learning that occurs through observing the behaviour of others.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">observational learning</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000001 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000195"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There is no intended sequence of the entities that are part of the emotion process . 

Emotion processes are distinguished from other affective processes  by having an object. Even if an emotion is experienced due to physiological reasons, they would be felt as being about something or someone. For instance, a person might be angry at a situation that they would normally not be angry at, because they are hungry. However, that person would still be angry at a specific situation. 

Valence refers to the &apos;the subjective value of an event, object, person, or other entity in the life space of the individual&apos; . Valence ranges from negative to positive.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An affective process that is a synchronized aggregate of constituent mental processes, including an appraisal process, which is valenced, has an object, and gives rise to an action tendency.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">emotion process</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000002 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000002">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that gives rise to an appraisal.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal process</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000003 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000003">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily process that encompasses all the neurophysiological changes accompanying an emotion, which take place in the central nervous system , neuro-endocrine system  and autonomous nervous system .</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">physiological process involved in an emotion</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000005 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Appraisals are a part of emotion processes, representing how a triggering stimulus is relevant to oneself. Appraisal captures the aboutness of an emotion process.

For instance, a stimulus  can produce different emotion processes depending on how the relevance of this stimulus is evaluated. The image of a tiger on TV would not be evaluated as dangerous. However, the image of a tiger two meters away from a person would be relevant to a person in terms of its dangerousness. Therefore, the appraisal of the dangerousness of the tiger would be part of the emotion process &apos;fear&apos;.

There is no intended sequence of the entities that are part of the emotion process .</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive representation of the emotional relevance of an object or event to the organism.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000006 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000006">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000195"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Subjective affective feeling can have greater or weaker physiological or mental components. The bodily process that are experienced will usually involve some mental process.

Bodily sensation and the experiences of emotions  would both qualify as examples of &apos;subjective affective feeling&apos;.

As a subclass of &apos;affective process&apos;, subjective affective feelings have valence. Valence refers to the &apos;the subjective value of an event, object, person, or other entity in the life space of the individual&apos; . Valence ranges from negative to positive.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An affective process that involves the experience of internal or external sensory stimuli.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> hunger</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> pain</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Feeling calm</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">subjective affective feeling</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000007 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000007">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000032"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Emotional action tendency involves dispositions that proceed a behaviour relating to emotions. For instance, fight or flight action tendencies influence a person&apos;s behaviour in response to a threat.</obo:IAO_0000112>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flight and fight action tendencies</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily disposition to behaviour that inheres in an organism by virtue of the physical changes brought about by an emotion process.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">emotional action tendency</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000009 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000009">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A negative emotion, characterised by feelings of unpleasantness and high arousal, in the form of antagonistic feelings and action tendencies.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">anger</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000019 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000019">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A negative emotion that is characterised by a need to get rid of, or distance oneself from, a stimulus that is appraised as repulsing.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">disgust</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000020 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000020">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disgust elicited by &quot;contaminated&quot; food substances: rotten food, culturally variable &quot;bad&quot; food, certain animals associated with rotten food , and every body product except tears.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">core disgust</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000021 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000021">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disgust elicited by poor hygiene, inappropriate sex, gore or violations of bodily boundaries, and death or the odor of decay.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">animal-nature disgust</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000022 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000022">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disgust elicited by contact with others, especially contact with strangers or other individuals or groups we are averse to.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">interpersonal disgust</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000023 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000023">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disgust elicited by some kinds of especially egregious moral violations.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">moral disgust</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000026 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000026">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Distinguished from anxiety since it usually has sudden onset and offset. Anxiety is often gradual and long-lasting and is triggered by a distal threat.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A negative emotion that is aversive and characterised by feelings of threat and impending doom, and by an urge to get out of the situation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fear</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000028 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000028">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Experiencing &apos;anxious mood&apos; can give rise to &apos;anxiety&apos;. An &apos;anxious mood&apos; does not have an object but involves generalised feeling of anxiety that a person experiences. In contrast, &apos;anxiety&apos; will be about something, even if this object is not well-specified.

The term &apos;distal&apos; can refer to future threats or geographically distant threats.

Distinguished from fear since it is triggered by a distal threat whereas fear is triggered by an immediate threat. Often gradual and long-lasting, whereas fear usually has sudden onset and offset.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A negative emotion that is aversive and provoked by the prospect of distal threat.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">anxiety</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000032 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000032">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An emotion caused by encountering unexpected events.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">surprise</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000035 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000035">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A positive emotion which is sought out and is associated with happiness and satisfaction.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">enjoyment</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pleasure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000040 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000040">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pleasure that is experienced as a result of sexual activities.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sexual pleasure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000042 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000042">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A positive emotion which is experienced in reaction to a positive experience or event.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">happiness</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000053 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000053">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A negative self-evaluative emotion that occurs when a person brings about a negative outcome by acts of commission or omission that violate internal or external moral standards.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">guilt</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000055 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000055">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A negative emotion that is distressing and occurs when one appraises one&apos;s behaviour or circumstance as wrong, dishonourable, immodest or indecorous.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">shame</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000056 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000056">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A negative emotion felt when an event is appraised as unpleasant and resulting in loss or failure.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sadness</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000060 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000060">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation of whether an event was expected to occur.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of expectedness</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000061 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000061">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation of the pleasantness of an object or event.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of pleasantness</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000072 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000072">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation of whether an event or object is important to the person&apos;s goals or needs.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of goal importance</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000075 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000075">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation of who or what caused an event.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of causal agency</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000076 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000076">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000075"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation that an event was caused by oneself.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal as caused by self</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000078 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000078">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000075"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation that an event was caused by another person.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal as caused by an other</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000080 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000080">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling of tiredness, needing sleep.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">feeling tired</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000085 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000085">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation of the desirability of the expected consequences of an event.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of desirability of consequences</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000091 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000091">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal which represents a judgement about how avoidable the expected consequences of an event will be.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of avoidability of consequences</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000103 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000103">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dangerousness or level of threat may refer to physical, emotional or social dangers or threats.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal which represents an evaluation of how threatening an object or situation is.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of dangerousness</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000107 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000107">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling of being at ease, comfortable, relaxed.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">feeling at ease</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000109 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000109">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling of having lots of energy, being energetic.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">feeling energetic</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000112 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000112">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000080"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Feeling tired to an extremely strong degree.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">feeling exhausted</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000124 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000124">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Note that &apos;feeling nervous&apos; is related to &apos;anxiety&apos; but is not identical with it. Feeling nervous is the subjective experience of nervousness, while anxiety is a complete emotion process. Subjective affective feelings are process parts of emotion processes.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling of being not at ease or agitated.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">feeling nervous</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000169 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000169">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An emotion that is experienced when one&apos;s wishes, expectations or needs are fulfilled.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">satisfaction</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000195 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000195">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Valence refers to the &apos;the subjective value of an event, object, person, or other entity in the life space of the individual&apos; . Valence ranges from negative to positive.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that has positive or negative valence.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">affective process</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000203 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000203">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling in an organism S, involving two integrated levels:  activation of the nociceptive system and associated emotion generating brain components of S, and  a simultaneous aversive sensory and emotional experience on the part of S, where  is phenomenologically similar to the sort of aversive experience involved in pain with concordant tissue damage.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pain</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000205 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000205">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling that involves discomfort and is associated with a need to consume food.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hunger</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000209 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000209">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000032"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Surprise with a positive valence.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> astonishment</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">wonder</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">positive surprise</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000210 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000210">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000032"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Surprise with a negative valence.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">negative surprise</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000006 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000006">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process during which information is evaluated, the outcome of which is a belief or opinion.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">judging</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000008 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000008">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that creates, modifies or has as participant some cognitive representation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cognitive process</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000012 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000012">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that involves heightened responding to an internal or external stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">arousal</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000013 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000013">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000008"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive process that involves the manipulation of mental language and/or mental images.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">thinking</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000017 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000017">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000144"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> and intends the object or event that the mental process is about, should such exist</obo:IAO_0000115>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> it confers intentionality on the mental process.</obo:IAO_0000115>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">That part of the mental process that confers a subjective perspective, a phenomenology, an experience of the mental process of which it is a part</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">consciousness</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000018 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000018">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process whereby relevant aspects of one&apos;s mental experience are focused on specific targets.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">attending</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000019 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000019">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Examples for &apos;causal processes involving a part of the environment of an organism&apos; that can be perceived are light rays and air vibrations.

&apos;Perception&apos; is about perceiving stimuli in one&apos;s environment in the MoA Ontology. However, the term &apos;perception&apos; is sometimes used to refer to things defined as &apos;belief&apos; in the MoA Ontology.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process which is a) produced by a causal process involving a part of the environment of the organism, and b) is experienced by the organism as being so caused, and c) in which the relevant part of the environment is thereby represented to the organism.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">perception</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000020 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000020">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mental processes can vary in the degree to which they involve consciousness.

A &apos;mental process&apos; is a subclass of &apos;bodily process&apos;, as mental processes occur within the body, specifically involving the brain.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily process that occurs in the brain, and that can of itself be conscious, or can give rise to a process that can of itself be conscious or can give rise to behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental process</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000027 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000027">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The outcome of this mental process is a mental plan which has the potential to be followed to bring about the desired outcome.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that involves mentally manipulating representations of steps in an imagined process which has some goal.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">planning</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000031 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000031">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050310"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The use of the phrase an anatomical structure in the cognitive system in the formal definition means that a specific cognitive representation will depend on parts of the brain, rather than involving the whole brain.

When a belief is activated, the content of that belief is a cognitive representation.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A representation which specifically depends on an anatomical structure in the cognitive system of an organism.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cognitive representation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000032 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000032">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extended organism refers to an organism and its microbiome 

Bodily disposition would include features, such as a person&apos;s strength , but not weight. A person would have a certain weight independent of most circumstances they are in.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A disposition that inheres in some extended organism.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bodily disposition</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000033 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000033">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000032"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mental disposition&apos; is a subclass of &apos;bodily disposition&apos;, as mental dispositions occur in the body.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily disposition that is realized in a mental process.</obo:IAO_0000115>
