    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050607 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050607">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050219"/>
