        <rdfs:label>cognitive representation of one’s behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050616 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050616">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006012"/>
