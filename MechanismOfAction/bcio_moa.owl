        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about social support&gt; for the person to enact a behaviour.</obo:IAO_0000115>
        <rdfs:label>belief about social support for a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050601 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050601">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
