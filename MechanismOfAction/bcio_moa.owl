    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050580 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050580">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050633"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Dissonance reduction through mental process&gt; where the cause of the dissonance involves cognitions about behaviour.</obo:IAO_0000115>
        <rdfs:label>behavioural dissonance reduction through mental process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050581 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050581">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <obo:IAO_0000112>&quot;Driving forces&quot; in the Change Theory</obo:IAO_0000112>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
