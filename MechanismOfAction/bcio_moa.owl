        <rdfs:label>behaviour maintenance self-efficacy</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050577 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050577">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
