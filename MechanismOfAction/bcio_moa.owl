    <!-- http://humanbehaviourchange.org/ontology/BCIO_050588 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050588">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
