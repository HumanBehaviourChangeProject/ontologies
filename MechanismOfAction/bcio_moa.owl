    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050583 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050583">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006086"/>
