    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0046959">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0046958"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>A decrease in a behavioural response to a repeated stimulus.</obo:IAO_0000115>
        <rdfs:label>habituation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0046960 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0046960">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0046958"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>An increase in a behavioural response to a repeated stimulus.</obo:IAO_0000115>
        <rdfs:label>sensitization</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/GO_0098596 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0098596">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0098597"/>
