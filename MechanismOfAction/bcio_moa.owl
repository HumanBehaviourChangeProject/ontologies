     xml:base="http://humanbehaviourchange.org/ontology/bcio_moa.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="https://schema.org/"
     xmlns:Property="https://www.wikidata.org/wiki/Property:"
     xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#"
     xmlns:owlready_ontology="http://www.lesfleursdunormal.fr/static/_downloads/owlready_ontology.owl#">
    <owl:Ontology rdf:about="http://humanbehaviourchange.org/ontology/bcio_moa.owl">
        <owl:imports rdf:resource="http://humanbehaviourchange.org/ontology/bcio_external.owl"/>
        <owl:imports rdf:resource="http://humanbehaviourchange.org/ontology/bcio_upper_level.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://addictovocab.org/ADDICTO_0000349 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000349">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; towards repeated episodes of abnormally high levels of motivation to engage in a behaviour, acquired as a result of engaging in the behaviour, where the behaviour results in risk or occurrence of serious net harm.</obo:IAO_0000115>
        <rdfs:comment>While &apos;addiction&apos; is a disposition relating to experience motivation to use a substance, &apos;substance dependence&apos; refers to &apos;disposition related to the impaired functioning in the absence of a substance&apos;.</rdfs:comment>
        <rdfs:label>addiction</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000377 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000377">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>A feeling involving positive affect associated with reduction in a feeling of need.</obo:IAO_0000115>
        <obo:IAO_0000118>subjective need</obo:IAO_0000118>
        <rdfs:label>feeling of need satisfaction</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000381 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000381">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive representation&gt; of themselves by a person or group.</obo:IAO_0000115>
        <rdfs:label>identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000399 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000399">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000381"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;identity&gt; that a person has about themselves.</obo:IAO_0000115>
        <rdfs:label>self-identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000511 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000511">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;subjective affective feeling&gt; involving a strong desire or urge.</obo:IAO_0000115>
        <rdfs:label>craving</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000715 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000715">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000381"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;identity&gt; that a group holds about itself.</obo:IAO_0000115>
        <rdfs:comment>An ongoing consideration regarding this class is whether a socially shared representation can be said to be purely cognitive.</rdfs:comment>
        <rdfs:label>group identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0001087 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0001087">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000399"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;self-identity&gt; in which the representation involves a relation between the identity holder and another person or group.</obo:IAO_0000115>
        <rdfs:comment>&apos;Social identity&apos; can involve a sense of one&apos;s rank or status in reference to other groups.</rdfs:comment>
        <rdfs:label>social identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0001140 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0001140">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000032"/>
        <obo:IAO_0000114>External</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;bodily disposition&gt; which is realised as impaired functioning following reduction or termination of use of a psychoactive substance.</obo:IAO_0000115>
        <rdfs:comment>While &apos;addiction&apos; is a disposition relating to experience motivation to use a substance, &apos;substance dependence&apos; refers to &apos;disposition related to the impaired functioning in the absence of a substance&apos;.</rdfs:comment>
        <rdfs:label>substance dependence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006001 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000254"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;environmental system&gt; that consists of people, their pets, social and cultural institutions and processes that influence the life of people and social groups.</obo:IAO_0000115>
        <rdfs:label>social environmental system</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006002 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006002">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006001"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;social environmental system&gt; that consists of persons related as members of a domestic group, or a number of domestic groups linked through descent from a common ancestor, marriage,  adoption or other legal arrangements.</obo:IAO_0000115>
        <rdfs:label>family environmental system</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006003 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006003">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006001"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;social environmental system&gt; that consists of persons who share a bond of mutual affection and are in contact or communication.</obo:IAO_0000115>
        <rdfs:label>friendship network</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006004 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006004">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000048"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental capability&gt; acquired through training or practice.</obo:IAO_0000115>
        <rdfs:comment>The term &apos;skill&apos; specifically refers to acquired capabilities, as behavioural theories suggested that the main difference between capabilities and skills was that skills were learned.</rdfs:comment>
        <rdfs:label>mental skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006005 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000048"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental capability&gt; that involves processes that modulate the frequency, rate or extent of a response to external or internal stimuli and that are instigated by the person themselves.</obo:IAO_0000115>
        <rdfs:comment>&apos;Modulate’ refers to ‘modifying or controlling influence on or change the strength’ and involves processes</rdfs:comment>
        <rdfs:label>self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006006 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006006">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;self-regulation capability&gt; to modulate one&apos;s behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118>behavioural regulation capability</obo:IAO_0000118>
        <rdfs:label>behavioural self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006007 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006007">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;self-regulation capability&gt; to modulate one&apos;s cognitive processes.</obo:IAO_0000115>
        <rdfs:label>cognitive self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006008 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006008">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;self-regulation capability&gt; to modulate one&apos;s emotional responses to a stimulus.</obo:IAO_0000115>
        <obo:IAO_0000118>emotional regulation capability</obo:IAO_0000118>
        <rdfs:label>emotional self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006009 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006009">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050215"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;behavioural capability&gt; that involves sensory or musculoskeletal abilities.</obo:IAO_0000115>
        <rdfs:label>physical behavioural capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006010 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006010">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006009"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;physical behavioural capability&gt; acquired through training or practice.</obo:IAO_0000115>
        <rdfs:label>physical skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006011 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006011">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050215"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;personal capability&gt; includes interpersonal behaviour in its realisation.</obo:IAO_0000115>
        <rdfs:label>social behavioural capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006012 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006012">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006011"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;social behavioural capability&gt; acquired through training or practice.</obo:IAO_0000115>
        <rdfs:label>social skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006014 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006014">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to perceive or experience oneself as isolated from and not meaningfully involved in social groups.</obo:IAO_0000115>
        <rdfs:label>social alienation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006015 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006015">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; that is realized by attending to events, objects or sensory patterns in experience.</obo:IAO_0000115>
        <rdfs:comment>The events attended to can include internal and external events.</rdfs:comment>
        <rdfs:label>awareness</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006016 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006016">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to commit to enact or not enact a behaviour.</obo:IAO_0000115>
        <rdfs:comment>The term ‘plan’ can sometimes be used to refer to intention and sometimes for instructional plans.</rdfs:comment>
        <rdfs:label>behavioural intention</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006017 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006017">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about a factor that could restrict or impede a person from engaging in a behaviour.</obo:IAO_0000115>
        <rdfs:label>belief about barriers to a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006018 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006018">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about the extent to which one&apos;s own behaviour is similar to that of referent others.</obo:IAO_0000115>
        <rdfs:label>belief about conformity to behavioural norms</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006019 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006019">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about consequences of an occurrence&gt; in terms of what results from or follows the performance of a behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118>response efficacy</obo:IAO_0000118>
        <rdfs:comment>Consequences can be either positive or negative.</rdfs:comment>
        <rdfs:label>belief about consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006020 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006020">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about consequences of behaviour&gt; in terms of their impact on the social environmental system.</obo:IAO_0000115>
        <rdfs:comment>Consequences can be either positive or negative.</rdfs:comment>
        <rdfs:label>belief about social consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006021 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006021">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about consequences of an occurrence&gt; in terms of the outcomes resulting from achieving a goal.</obo:IAO_0000115>
        <rdfs:comment>Consequences can be either positive or negative.

Belief about consequences of goal attainment is about what types of outcomes will be achieved, rather than the value or the likelihood of these outcomes. For instance, a person may believe that studying every night will help achieve good grades. However, the value of achieving such grade may vary, and would fall under &apos;evaluative belief&apos;.</rdfs:comment>
        <rdfs:label>belief about consequences of goal attainment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006022 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006022">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about the extent to which one can direct what happens in one&apos;s future.</obo:IAO_0000115>
        <rdfs:label>belief about control over one&apos;s future</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006023 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006023">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about the existence of an increase from an initial reference point.</obo:IAO_0000115>
        <obo:IAO_0000118>Perceived gain</obo:IAO_0000118>
        <rdfs:comment>The perceived gain could be in anything internal or external, such as health or wealth.

The classes &apos;belief about gain&apos; and &apos;belief about reduction&apos; do not necessarily involve likelihood. Likelihoods relate to the future, However, &apos;belief about gain&apos; might be about an existing gain.  For instance, in situation where the following trade is offered: &quot;I&apos;ve given you $2, are you willing to trade it for a 50% chance of winning $6?&quot; it&apos;d be &quot;I have $2 more than I started with&quot;, there is an existing gain that is compared to a future one.</rdfs:comment>
        <rdfs:label>belief about gain</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006024 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006024">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006026"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about likelihood of consequences of an occurrence&gt; in terms of the probability that a behaviour will result or not result in particular outcomes.</obo:IAO_0000115>
        <rdfs:label>belief about likelihood of consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006025 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006025">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006024"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about likelihood of consequences of behaviour&gt; in terms of the probability of a behaviour resulting or not resulting in a change to health or wellbeing.</obo:IAO_0000115>
        <rdfs:label>belief about likelihood of health consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006026 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006026">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about consequences of an occurrence&gt; in terms the probability that a given event or state will occur or not occur in the future.</obo:IAO_0000115>
        <rdfs:comment>Consequences can be either positive or negative.</rdfs:comment>
        <rdfs:label>belief about likelihood of consequences of an occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006027 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006027">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about the existence of a decrease from an initial reference point.</obo:IAO_0000115>
        <obo:IAO_0000118>belief about loss</obo:IAO_0000118>
        <rdfs:comment>The perceived loss could be in anything internal or external, such as wealth or health.

The classes &apos;belief about gain&apos; and &apos;belief about reduction&apos; do not necessarily involve likelihood. Likelihoods relate to the future. However, &apos;belief about reduction&apos; might be about an existing loss.</rdfs:comment>
        <rdfs:label>belief about reduction</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006028 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006028">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006141"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about message&gt; in terms of whether the content of a verbal, graphic, written or recorded communication is relevant to oneself.</obo:IAO_0000115>
        <rdfs:label>belief about message relevance</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006029 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006029">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about the nature of the immediate physical and social settings in which people live, including  culture and the people and institutions with whom they interact.</obo:IAO_0000115>
        <rdfs:label>belief about ones environment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006030 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006030">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about how serious the harm associated with an outcome could be.</obo:IAO_0000115>
        <obo:IAO_0000118>belief about threat severity</obo:IAO_0000118>
        <rdfs:label>belief about severity of an outcome</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006031 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006031">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006305"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about susceptibility to a threat&gt; in terms of one&apos;s personal vulnerability to a threat.</obo:IAO_0000115>
        <rdfs:label>belief about personal susceptibility</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006033 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006033">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about consequences of behaviour&gt; in terms of how long after the performance of the behaviour they will occur.</obo:IAO_0000115>
        <rdfs:label>belief about the timing of the consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006034 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006034">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about the extent to which one is free to choose whether to perform the behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118>belief about autonomy in performing behaviours</obo:IAO_0000118>
        <rdfs:label>belief about choice over a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006035 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006035">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006038"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;evaluative belief&gt; about one&apos;s attributes.</obo:IAO_0000115>
        <obo:IAO_0000118>Self-concept</obo:IAO_0000118>
        <rdfs:comment>One&apos;s attributes can be evaluated as positive or negative.</rdfs:comment>
        <rdfs:label>evaluation of self</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006038 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006038">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about whether a particular aspect of the world is positive or negative.</obo:IAO_0000115>
        <rdfs:comment>The class &apos;evaluative belief about the consequences of behaviour&apos; can be found as the subclass of &apos;belief about consequences of behaviour&apos;.</rdfs:comment>
        <rdfs:label>evaluative belief</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006039 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006039">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about one&apos;s social environment&gt; in terms of what is typical for people who belong to a particular group.</obo:IAO_0000115>
        <obo:IAO_0000118>belief about norm</obo:IAO_0000118>
        <rdfs:comment>A person does not need to belong to a group to have a perception of their norms. For instance, the perceived norm could be in a group one aspires to belong to, and so the person might attempt to conform to it in an attempt to be accepted as a member of that group.</rdfs:comment>
        <rdfs:label>perceived norm</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006040 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006040">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006039"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;perceived norm&gt; regarding the prevalence of performance of a given behaviour by people within a group.</obo:IAO_0000115>
        <rdfs:label>perceived descriptive norm</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006041 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006041">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006039"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;perceived norm&gt; regarding whether a behaviour is appropriate and correct for people who belong to a particular group.</obo:IAO_0000115>
        <rdfs:label>perceived injunctive norm</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006042 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006042">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006039"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;perceived norm&gt; regarding whether key others think one should perform a behaviour.</obo:IAO_0000115>
        <rdfs:comment>&apos;Key others&apos; can include &apos;people in one’s social network’</rdfs:comment>
        <rdfs:label>normative belief</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006043 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006043">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about one&apos;s capabilities to organise and execute a behaviour and achieve the outcomes associated with this behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118>self-efficacy</obo:IAO_0000118>
        <rdfs:comment>The phrase &apos;to organise&apos; refers to the ability to organise mental processes and sequences of actions.

&apos;self-efficacy belief for a behaviour&apos; is part of &apos;self-efficacy belief for a behaviour and its associated outcomes&apos;. However, the latter also includes a belief about the outcomes associated with behaviour.</rdfs:comment>
        <rdfs:label>self-efficacy belief for a behaviour and its associated outcomes</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006044 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006044">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006154"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;self-efficacy belief for a behaviour&gt; when a barrier or facilitator of the behaviour is encountered.</obo:IAO_0000115>
        <rdfs:comment>The &apos;barrier or facilitator&apos; specified in the definition can be external or internal.</rdfs:comment>
        <rdfs:label>situational self-efficacy belief for a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006045 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006045">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; that when activated, guides an interconnected network of perception, thought, emotion or behaviour.</obo:IAO_0000115>
        <rdfs:label>cognitive schema</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006046 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006046">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006045"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive schema&gt; that when activated, guides a sequence of behaviours.</obo:IAO_0000115>
        <obo:IAO_0000118>action script</obo:IAO_0000118>
        <rdfs:label>action schema</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006047 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006047">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to represent one proposition as preferred from at least two.</obo:IAO_0000115>
        <rdfs:comment>Decision is defined as a &apos;mental disposition&apos;, a realisable entity, because a person requires a process to form, think about or reconsider a decision.</rdfs:comment>
        <rdfs:label>decision</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006048 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006048">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006047"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;decision&gt; that one behaviour is preferred from at least two behavioural options.</obo:IAO_0000115>
        <rdfs:label>behavioural decision</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006049 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006049">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive representation&gt; of an end state towards which one is striving.</obo:IAO_0000115>
        <rdfs:label>goal</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006052 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006052">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to understand the nature of the world, or a specific aspect of the world, that corresponds to the actual state of the world and is acquired through experience or learning.</obo:IAO_0000115>
        <rdfs:comment>Knowledge is defined as a &apos;mental disposition&apos;, a realisable entity, because a person requires a process to access the information stored in their brain. A person knows something, when the relevant information has been retrieved from their memory. There are instances where a person might have learned something, but is unable to retrieve the information at a specific moment.</rdfs:comment>
        <rdfs:label>knowledge</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006053 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006053">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Knowledge&gt; regarding a particular set of actions.</obo:IAO_0000115>
        <rdfs:label>knowledge regarding a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006054 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006054">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Knowledge&gt; regarding a novel invention or way of doing something.</obo:IAO_0000115>
        <rdfs:label>knowledge regarding an innovation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006055 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006055">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Knowledge&gt; regarding a potential harm.</obo:IAO_0000115>
        <rdfs:label>knowledge regarding a threat</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006056 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006056">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Knowledge&gt; regarding a therapy or medication for a health problem.</obo:IAO_0000115>
        <rdfs:label>knowledge regarding treatment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006057 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006057">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to think or behave in a particular way in response to an internal or external event in the person&apos;s environment, which is acquired through associative learning.</obo:IAO_0000115>
        <rdfs:comment>The responses to an event can include thinking and impulses to respond.</rdfs:comment>
        <rdfs:label>learned stimulus-reponse co-occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006058 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006058">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to evoke the representation of the sensory characteristics of objects or events when these are not immediately present to the senses.</obo:IAO_0000115>
        <rdfs:label>mental imagery disposition</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006059 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006059">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to act in accordance with the likely approval of others.</obo:IAO_0000115>
        <obo:IAO_0000118>motivation to comply</obo:IAO_0000118>
        <rdfs:label>willingness to comply</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006060 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006060">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; for motivation to be guided by a focus on the presence or absence of outcomes of a certain valence.</obo:IAO_0000115>
        <rdfs:label>motivational orientation towards types of outcomes</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006061 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006061">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006060"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;motivational orientation towards types of outcomes&gt; in which motivation is guided by a focus on the presence or absence of negative outcomes.</obo:IAO_0000115>
        <rdfs:label>prevention focused motivational orientation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006062 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006062">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006060"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;motivational orientation towards types of outcomes&gt; in which motivation is guided by a focus on the presence or absence of positive outcomes.</obo:IAO_0000115>
        <rdfs:label>promotion focused motivational orientation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006063 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006063">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to regard certain things as fundamentally important in life, which informs standards for behaviour.</obo:IAO_0000115>
        <rdfs:label>personal value</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006064 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006064">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; of a person to act to obtain or maintain a particular state due to this state’s importance to the person’s wellbeing.</obo:IAO_0000115>
        <rdfs:comment>Psychological needs can be satisfied or thwarted.</rdfs:comment>
        <rdfs:label>psychological need</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006065 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006065">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;psychological need&gt; to have a sense of personal agency and initiative.</obo:IAO_0000115>
        <rdfs:label>need for autonomy</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006066 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006066">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;psychological need&gt; to have a positive evaluation of oneself and one&apos;s qualities and characteristics.</obo:IAO_0000115>
        <rdfs:label>need for self-esteem</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006067 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006067">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;psychological need&gt; to feel safe from threats.</obo:IAO_0000115>
        <rdfs:label>need for a sense of safety</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006068 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006068">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;psychological need&gt; to have caring relationships with other people.</obo:IAO_0000115>
        <rdfs:label>need for a sense of belonging and attachment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006069 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006069">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;psychological need&gt; to experience oneself as capable and effective.</obo:IAO_0000115>
        <obo:IAO_0000118>need for mastery</obo:IAO_0000118>
        <rdfs:label>need to feel competent</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006070 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006070">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050228"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental plan&gt; that is realised in mental processes representing the steps to enact a behaviour and the conditions under which the behaviour will be enacted.</obo:IAO_0000115>
        <obo:IAO_0000118>Action plan</obo:IAO_0000118>
        <rdfs:comment>The term ‘plan’ can sometimes be used to refer to intention and sometimes for instructional plans.

&apos;mental plan for a behaviour’ involves  plans for which details about the behaviour (when [specific times], where, how or with whom) are specified.</rdfs:comment>
        <rdfs:label>mental plan for a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006072 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006072">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006045"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive schema&gt; that when activated, guides perception, thought, emotion or behaviour toward members of a social group as having certain shared attributes, by virtue of their group membership.</obo:IAO_0000115>
        <rdfs:comment>The attributes of members are not definitional. For instance, a psychologist studying psychology is not a stereotype.</rdfs:comment>
        <rdfs:label>stereotype</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006074 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006074">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental disposition&gt; to experience a feeling of being connected by social attachments.</obo:IAO_0000115>
        <rdfs:label>social embeddedness</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006078 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006078">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;appraisal&gt; that represents an evaluation of how much one is personally obliged to respond to an event or person in need.</obo:IAO_0000115>
        <rdfs:label>appraisal of obligation to act</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006079 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006079">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive representation&gt; of a reference level that an individual wishes to obtain.</obo:IAO_0000115>
        <rdfs:comment>Examples of &apos;reference level&apos; are the level of a behaviour and the blood glucose level.</rdfs:comment>
        <rdfs:label>desired standard</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006080 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006080">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive representation&gt; of the sensory characteristics of objects or events that are not immediately present to the senses.</obo:IAO_0000115>
        <rdfs:label>mental image</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006081 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006081">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;role&gt; that inheres in a human being by virtue of their social and institutional circumstances.</obo:IAO_0000115>
        <rdfs:comment>A role can be assigned without being realised. A person realises a role by doing something.</rdfs:comment>
        <rdfs:label>personal role</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006082 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006082">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006081"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;personal role&gt; that is realised in human social processes.</obo:IAO_0000115>
        <rdfs:comment>A role can be assigned without being realised. A person realises a role by doing something.</rdfs:comment>
        <rdfs:label>social role</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006085 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006085">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A spatial &lt;quality&gt; that inheres in a bearer by virtue of its position relative to other entities.</obo:IAO_0000115>
        <rdfs:comment>An intervention could be used to rearrange objects in an environment, changing the location of a person relative to these objects and thereby increasing the likelihood of a behaviour.</rdfs:comment>
        <rdfs:label>location</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006086 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006086">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000452"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;environmental disposition&gt; that is required for or facilitates a behaviour.</obo:IAO_0000115>
        <rdfs:comment>The phrase ‘required for’ is used in the definition to specify that &apos;behavioural opportunity&apos; is a necessary condition for behaviour to occur. Although other MoAs will help bring behaviour about, there has to be a &apos;behavioural opportunity&apos; for behaviour to occur.

The term &apos;facilitates&apos; is used to specify that &apos;behavioural opportunity&apos; will make the behaviour easier. Higher behavioural opportunity will increase the likelihood of behaviour across situations and people.

The class &apos;environmental system&apos; relates to what is found in the physical and social environment (e.g., how many streets are there for you to walk on), while the class &apos;behavioural opportunity&apos; is about the features of the environment and how they interact with the person (e.g., whether an environment enables a person to take walks).</rdfs:comment>
        <rdfs:label>behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006087 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006087">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006089"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;physical behavioural opportunity&gt; that involves the person having sufficient financial resources.</obo:IAO_0000115>
        <rdfs:comment>As &apos;financial behavioural opportunity&apos; will involve some physical monetary entity (e.g., money borrowed from someone), it is a physical behavioural opportunity. While financial resources (e.g., money) have intrinsically social dimensions, the &apos;financial behavioural opportunity&apos; does not involve a social aspect.</rdfs:comment>
        <rdfs:label>financial behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006088 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006088">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006086"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;behavioural opportunity&gt; regarding how easy it is for a person to approach and use a healthcare service.</obo:IAO_0000115>
        <rdfs:comment>The &apos;healthcare services&apos; refers to services provided for the purposes of preventing, detecting, curing or controlling a health problem or enhancing wellbeing.</rdfs:comment>
        <rdfs:label>healthcare access</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006089 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006089">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006086"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;behavioural opportunity&gt; that involves time and parts of the environmental system that do not involve people or organisations.</obo:IAO_0000115>
        <rdfs:comment>A &apos;behavioural opportunity&apos; is a feature of the environmental system. While the class &apos;environmental system&apos; is about what is found in an environment, the behavioural opportunity is a feature of the environment which interacts with the person and enables or prevents a behaviour.</rdfs:comment>
        <rdfs:label>physical behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006090 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006090">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006086"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;behavioural opportunity&gt; that involves the social environmental system.</obo:IAO_0000115>
        <rdfs:label>social behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006091 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006091">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006089"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;physical behavioural opportunity&gt; that involves the person having sufficient time or a suitable period.</obo:IAO_0000115>
        <rdfs:label>temporal behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006092 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006092">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;bodily process&gt; that arises from another bodily process and serves to elicit or guide behaviour.</obo:IAO_0000115>
        <rdfs:label>bodily behavioural cue</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006093 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006093">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006092"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;bodily behavioural cue&gt; that arises from mental processes and serves to elicit or guide behaviour.</obo:IAO_0000115>
        <rdfs:label>mental behavioural cue</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006095 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006095">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036000"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;individual human behaviour&gt; that is commonly enacted by people that are part of a social environmental system.</obo:IAO_0000115>
        <rdfs:comment>Normative behaviour can be an MoA or outcome depending on how &apos;normative behaviour&apos; is described in an intervention. The Behaviour Change-Intervention Ontology includes the class &apos;outcome behaviour&apos; (definition: Human behavior that is an intervention outcome.), which would not be an MoA. When an intervention targets a normative behaviour to achieve an outcome behaviour, the former qualifies as an MoA.</rdfs:comment>
        <rdfs:label>normative behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006096 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006096">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;bodily process&gt; in which attempts are made to achieve a desired end state.</obo:IAO_0000115>
        <rdfs:comment>&apos;Goal pursuit process&apos; is a bodily process, as it may involve more than just mental processes (i.e., other processes in the body).</rdfs:comment>
        <rdfs:label>goal pursuit process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006099 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006099">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036025"/>
        <obo:IAO_0000112>Social support from social network.</obo:IAO_0000112>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;inter-personal behaviour&gt; where a person exerts an influence on the behaviour of another.</obo:IAO_0000115>
        <rdfs:label>social influence behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006100 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006100">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;bodily process&gt; by which the person experiences an internally-generated positive physical or psychological state subsequent to a response.</obo:IAO_0000115>
        <rdfs:label>internal reward for a response</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006102 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006102">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;bodily process&gt; by which a person attempts to follow the steps in a plan.</obo:IAO_0000115>
        <rdfs:label>plan enactment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006103 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006103">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050268"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;self-regulation process&gt; that modulates the frequency, rate or extent of one&apos;s  performance of a behaviour.</obo:IAO_0000115>
        <rdfs:label>self-regulation of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006104 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006104">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Self-regulation of behaviour&gt; through the performance of a different behaviour.</obo:IAO_0000115>
        <rdfs:comment>Behavioural self-regulation of behaviour can be an MoA or outcome depending on how &apos;behavioural self-regulation of behaviour&apos; is described in an intervention. The Behaviour Change-Intervention Ontology includes the class &apos;outcome behaviour&apos; (definition: Human behavior that is an intervention outcome.), which would not be an MoA. When an intervention targets behavioural self-regulation of behaviour to achieve an outcome behaviour, the former qualifies as an MoA.</rdfs:comment>
        <rdfs:label>behavioural self-regulation of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006105 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006105">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Self-regulation of behaviour&gt; undertaken to manage a sense of guilt, anxiety or obligation.</obo:IAO_0000115>
        <rdfs:label>introjected self-regulation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006106 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006106">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Self-regulation of behaviour&gt; through self-administering a reward for performing an intended behaviour.</obo:IAO_0000115>
        <rdfs:label>self-reinforcing self-regulation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006107 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006107">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Self-regulation of behaviour&gt; undertaken to achieve consistency between one&apos;s values and the behaviour performed.</obo:IAO_0000115>
        <rdfs:label>value-congruent self-regulation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006108 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006108">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;subjective affective feeling&gt; that is unpleasant and experienced due to perceived inconsistency between two concurrently held cognitive representations.</obo:IAO_0000115>
        <rdfs:label>cognitive dissonance</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006109 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006109">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;subjective affective feeling&gt; that  arises when an impulse to enact a behaviour is activated but not immediately acted upon.</obo:IAO_0000115>
        <rdfs:comment>An urge is felt when an impulse is frustrated.</rdfs:comment>
        <rdfs:label>feeling an urge</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006110 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006110">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000008"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive process&gt; that creates a representation of the meaning and significance of a communication, object, event or situation.</obo:IAO_0000115>
        <rdfs:label>comprehension</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006111 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006111">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000008"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive process&gt; used prior to decision-making that reduces the perceived scope or complexity of available options.</obo:IAO_0000115>
        <rdfs:label>decision simplification</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006112 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006112">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000013"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Thinking&gt; that involves the generation of inferences by reasoning based on one or more statements, evaluating anticipated consequences or considering the relationships between concepts.</obo:IAO_0000115>
        <rdfs:label>reflective thinking</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006113 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006113">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; through which a perceived inconsistency between two concurrently held cognitive representations is reduced.</obo:IAO_0000115>
        <rdfs:label>dissonance reduction process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006114 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006114">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; that establishes a cognitive representation of the desired end state.</obo:IAO_0000115>
        <rdfs:label>goal setting process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006115 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006115">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; that uses simple rules and associations learnt from experience to make judgements.</obo:IAO_0000115>
        <rdfs:label>heuristic process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006116 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006116">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000006"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Judging&gt; during which one proposition is selected as preferred from at least two.</obo:IAO_0000115>
        <rdfs:comment>The output of decision-making is a decision.</rdfs:comment>
        <rdfs:label>decision-making</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006118 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006118">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000006"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Judging&gt; oneself or one&apos;s social group in relation to the qualities or characteristics of another person or social group.</obo:IAO_0000115>
        <rdfs:label>social comparison process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006119 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006119">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050239"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Learning&gt; that involves changing the strength of association between two or more mental processes.</obo:IAO_0000115>
        <rdfs:comment>While associative learning is often defined as learning an association between a stimulus and response, the stimulus has to be perceived. Therefore, associative learning is between perceiving or processing information about a stimulus (a mental process) and one or more other mental processes.

The label &apos;associative learning&apos; is also used for a behaviour change technique (BCT) in the BCT Taxonomy v1 (Michie et al., 2013). The definition for this BCT reads as follows: &apos;Present a neutral stimulus jointly with a stimulus that already elicits the behavior repeatedly until the neutral stimulus elicits that behavior.&apos; As indicated by the definition, the BCT is the deliberate presentation of a stimulus, whereas the MoA is a learning process that occurs in the brain.</rdfs:comment>
        <rdfs:label>associative learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006120 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006120">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Associative learning&gt; that occurs when the pairing of a conditioned and unconditioned stimulus leads to the conditioned stimulus producing a response that would otherwise have only been produced by the unconditioned stimulus.</obo:IAO_0000115>
        <rdfs:label>classical conditioning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006121 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006121">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Associative learning&gt; in which people learn to distinguish between appropriate responses to similar but different stimuli depending on whether a generalised response is rewarded or not.</obo:IAO_0000115>
        <rdfs:label>discrimination learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006122 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006122">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Associative learning&gt; in which the strength of a conditioned response is reduced because its conditioned stimulus is presented without being accompanied by the unconditioned stimulus.</obo:IAO_0000115>
        <rdfs:comment>An unconditioned stimulus is &quot;a stimulus that elicits a reflexive or innate response (the unconditioned response) without prior learning.&quot; (p., 280, Holt, N., Bremner, A., Sutherland, E., Vliek, M., Passer, M., and Smith, R. (2019). EBOOK: Psychology: The Science of Mind and Behaviour, 4e. McGraw Hill.)

A conditioned stimulus is a stimulus that, through association with an unconditioned stimulus, comes to elicit a conditioned response similar to the original unconditioned response (Holt, N., Bremner, A., Sutherland, E., Vliek, M., Passer, M., and Smith, R. (2019). EBOOK: Psychology: The Science of Mind and Behaviour, 4e. McGraw Hill.)</rdfs:comment>
        <rdfs:label>extinction in classical conditioning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006123 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006123">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Associative learning&gt; in which the rate of behavioural response in the presence of a discriminative stimulus is reduced because the response is no longer followed by a reinforcer.</obo:IAO_0000115>
        <rdfs:comment> Holt, N., Bremner, A., Sutherland, E., Vliek, M., Passer, M., and Smith, R. (2019). EBOOK: Psychology: The Science of Mind and Behaviour, 4e. McGraw Hill.)</rdfs:comment>
        <rdfs:comment>Discriminative stimulus is &quot;a signal that a particular response will produce certain consequences.&quot; (p. 291</rdfs:comment>
        <rdfs:label>extinction in operant learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006124 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006124">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Associative learning&gt; in which prior learning that an antecedent stimulus signals certain consequences of behaviour spreads to a stimulus that differs in certain aspects from the original antecedent stimulus.</obo:IAO_0000115>
        <rdfs:comment>The label &apos;Generalisation of target  behavior&apos; is used for a behaviour change technique (BCT) in the BCT Taxonomy v1 (Michie et al., 2013). The definition for this BCT reads as follows: &apos;Advise to perform the wanted behaviour,  which is already performed in a particular situation, in another situation.&apos; Therefore, the BCT is the advice to perform the behaviour, whereas the MoA is a learning process that occurs in the brain.</rdfs:comment>
        <rdfs:label>generalisation in learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006125 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006125">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Associative learning&gt; in which the likelihood of a particular behavioural response is influenced by the past consequences associated with performing that behaviour.</obo:IAO_0000115>
        <rdfs:label>operant conditioning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006126 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006126">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050319"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;memory process&gt; that deals with the receipt, storage, retrieval and modification of information regarding a set of entities associated with a particular object or event.</obo:IAO_0000115>
        <rdfs:label>associative memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006127 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006127">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050319"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;memory process&gt; that deals with the receipt, storage, retrieval and modification of information regarding a personally experienced event at a particular time or place.</obo:IAO_0000115>
        <rdfs:label>episodic memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006128 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006128">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050319"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;memory process&gt; that deals with the receipt, storage, retrieval and modification of information associated with meanings, understandings or conceptual facts about the world.</obo:IAO_0000115>
        <rdfs:label>semantic memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006129 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006129">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050319"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;memory process&gt; that deals with the receipt, storage, retrieval and modification of information regarding how to perform a complex activity without conscious awareness.</obo:IAO_0000115>
        <rdfs:label>procedural memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006130 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006130">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050319"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;memory process&gt; that deals with the receipt, storage, retrieval and modification of information regarding a brief visual stimulus.</obo:IAO_0000115>
        <rdfs:label>iconic memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006131 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006131">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; in which objects, events, people, or experiences are grouped into classes, on the basis of features shared by members of the same class and features distinguishing the members of one class from those of another.</obo:IAO_0000115>
        <rdfs:label>mental categorising</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006132 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006132">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; in which conditions are compared against a particular reference level.</obo:IAO_0000115>
        <rdfs:comment>The term &apos;condition&apos; may refer to the characteristics of an object, event and person.</rdfs:comment>
        <rdfs:label>mentally comparing against a standard</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006133 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006133">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; that energises and directs a behaviour.</obo:IAO_0000115>
        <rdfs:label>behavioural motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006134 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006134">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006133"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Behaviour motivation&gt; that arises from emotions and impulses that result from associative learning or innate dispositions.</obo:IAO_0000115>
        <rdfs:label>automatic motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006136 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006136">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006302"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Autonomous motivation&gt; due to a behaviour&apos;s inherent enjoyment, satisfaction or benefits to oneself.</obo:IAO_0000115>
        <rdfs:comment>The phrase &apos;benefits to oneself&apos; includes the reduction of unpleasant internal states.</rdfs:comment>
        <rdfs:label>intrinsic motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006137 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006137">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; in which one observes one&apos;s own behaviour or mental processes.</obo:IAO_0000115>
        <rdfs:comment>There are behaviour change techniques labelled &apos;Self-monitoring of behaviour&apos; and &apos;Self-monitoring of outcome of behaviour&apos;. These are about establishing a method for monitoring states. In contrast, the MoA is the mental process of monitoring.

&apos;Self-monitoring of behaviour&apos; is defined as &apos;Instruct self recording of specified behaviour/s (with or without associated thoughts, emotions, situations) as part of a behaviour change strategy &apos; (Michie et al., 2013).
&apos;Self-monitoring of outcome of behaviour&apos; is defined &quot;Establish a method for the person to monitor and record their behavior(s) as part of a behavior change strategy&quot;  (Michie et al., 2013).</rdfs:comment>
        <rdfs:label>self-monitoring</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006140 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006140">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about consequences of an occurrence&gt; in terms of the potential emotions experienced as result of an event.</obo:IAO_0000115>
        <rdfs:comment>&apos;Belief about anticipated emotions&apos; is about the consequences of anything, not just behaviour. In contrast, the class &apos;belief about emotional consequences of behaviour&apos; is about the emotions resulting from behaviours.</rdfs:comment>
        <rdfs:label>belief about anticipated emotion</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006141 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006141">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; regarding aspects of a verbal, graphic, written or recorded communication.</obo:IAO_0000115>
        <rdfs:label>belief about message</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006142 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006142">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006141"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about message&gt; in terms of whether a verbal, graphic, written or recorded communication is provided by people or institutions who provide reliable information.</obo:IAO_0000115>
        <rdfs:label>belief about the credibility of a message&apos;s source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006143 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006143">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006141"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about message&gt; in terms of whether a verbal, graphic, written or recording communication provides information that can be relied on as honest or truthful.</obo:IAO_0000115>
        <rdfs:label>belief about message trustworthiness</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006144 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006144">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; regarding whether one or more of the person&apos;s needs have been met.</obo:IAO_0000115>
        <rdfs:label>belief about need satisfaction</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006145 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006145">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006029"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about one&apos;s environment&gt; in terms of the aspects of one’s immediate social settings in which people live, including culture and the people and institutions with whom they interact.</obo:IAO_0000115>
        <rdfs:label>belief about ones social environment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006146 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006146">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006145"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about one&apos;s social environment&gt; in terms of whether others will provide or are available to provide assistance or comfort to help the person cope with one or more stressors or enhance their wellbeing.</obo:IAO_0000115>
        <rdfs:label>belief about social support</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006147 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006147">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006038"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;evaluative belief&gt; about whether a behaviour is positive or negative.</obo:IAO_0000115>
        <rdfs:comment>For &apos;evaluative belief about the consequences of a behaviour&apos; see under parent class &apos;belief about the consequences of a behaviour&apos;.</rdfs:comment>
        <rdfs:label>evaluative belief about a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006148 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006148">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about consequences of behaviour&gt; in terms of whether they are positive or negative.</obo:IAO_0000115>
        <rdfs:comment>Note that while this has &apos;belief about the consequences of a behaviour&apos; as the parent, there is also a class &apos;evaluative belief&apos; that acts as a parent for other evaluative beliefs.</rdfs:comment>
        <rdfs:label>evaluative belief about the consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006149 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006149">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006148"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;evaluative belief about the consequences of behaviour&gt; in terms of the value of the consequences of behaviour to oneself.</obo:IAO_0000115>
        <rdfs:comment>&apos;Desirability&apos; can range from highly desirable to highly undesirable.</rdfs:comment>
        <rdfs:label>belief about the personal desirability of consequences of a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006150 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006150">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006038"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;evaluative belief&gt; about the attributes of others.</obo:IAO_0000115>
        <rdfs:label>evaluative belief about others</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006152 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006152">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about one&apos;s capabilities and opportunities within an environmental system to perform a behaviour or achieve the outcomes resulting from a behaviour.</obo:IAO_0000115>
        <rdfs:label>belief about control over behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006154 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006154">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about one&apos;s capabilities to organise and execute a behaviour.</obo:IAO_0000115>
        <rdfs:comment>The phrase &apos;to organise&apos; refers to the ability to organise mental processes and sequences of actions.

&apos;self-efficacy belief for a behaviour&apos; is part of &apos;self-efficacy belief for a behaviour and its associated outcomes&apos;. However, the latter also includes a belief about the outcomes associated with behaviour.</rdfs:comment>
        <rdfs:label>self-efficacy belief for a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006155 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006155">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006057"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;learned stimulus-response co-occurrence&gt; that involves a behavioural disposition in response to an internal or external event in the person&apos;s environment, which is acquired through habit learning.</obo:IAO_0000115>
        <obo:IAO_0000118>Habit</obo:IAO_0000118>
        <rdfs:comment>Learned stimulus-behaviour co-occurrence can vary in strength.</rdfs:comment>
        <rdfs:label>learned stimulus-behaviour co-occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006156 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006156">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006057"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;learned stimulus-response co-occurrence&gt; that involves a disposition to think in a particular way in response to an internal or external event in the person&apos;s environment, which is acquired through associative learning.</obo:IAO_0000115>
        <obo:IAO_0000118>Mental habit</obo:IAO_0000118>
        <rdfs:label>learned stimulus-thought co-occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006157 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006157">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006070"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental plan for a behaviour&gt; regarding a strategy to deal with conditions that could prevent the performance of a behaviour.</obo:IAO_0000115>
        <rdfs:label>mental plan for coping with barriers</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006158 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006158">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036000"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;individual human behaviour&gt; that results from a learnt stimulus-behaviour co-occurrence.</obo:IAO_0000115>
        <rdfs:comment>A learnt stimulus-behaviour co-occurrence can lead to activation or execution of a habitual behaviour.

Habitual behaviour can be an MoA or outcome depending on how &apos;habitual behaviour&apos; is described in an intervention. The Behaviour Change-Intervention Ontology includes the class &apos;outcome behaviour&apos; (definition: Human behavior that is an intervention outcome.), which would not be an MoA. When an intervention targets a habitual behaviour to achieve an outcome behaviour, the former qualifies as an MoA.</rdfs:comment>
        <rdfs:label>habitual behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006159 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006159">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000035"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Pleasure&gt; that is experienced as a result of engaging in a behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118>Enjoyment of behaviour</obo:IAO_0000118>
        <rdfs:label>pleasure associated with behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006161 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006161">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;mental process&gt; that reduces the frequency by which an aversive cognitive representation is evoked.</obo:IAO_0000115>
        <rdfs:label>avoidance mental process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006162 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006162">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000008"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;cognitive process&gt; that involves intentionally detaching oneself from one&apos;s immediate thoughts.</obo:IAO_0000115>
        <obo:IAO_0000118>decentring</obo:IAO_0000118>
        <rdfs:label>mental distancing</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006163 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006163">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006162"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Mental distancing&gt; that involves identifying particular immediate thoughts and intentionally detaching from these thoughts.</obo:IAO_0000115>
        <obo:IAO_0000118>targeted decentring</obo:IAO_0000118>
        <rdfs:label>targeted mental distancing</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006300 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006300">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006307"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Extrinsic motivation&gt; to gain or maintain social approval or a positive self-evaluation.</obo:IAO_0000115>
        <obo:IAO_0000118>introjected regulation</obo:IAO_0000118>
        <rdfs:label>approval-based motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006301 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006301">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006305"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief about susceptibility to a threat&gt; in terms of how vulnerable others are to a threat.</obo:IAO_0000115>
        <rdfs:label>belief about others&apos; susceptibility</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006302 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006302">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006133"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>&lt;Behavioural motivation&gt; that is internally generated and based on a behaviour being interesting or valuable to an individual.</obo:IAO_0000115>
        <rdfs:label>autonomous motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006303 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006303">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A &lt;belief&gt; about whether oneself or another person is obligated to take a certain course of action.</obo:IAO_0000115>
        <rdfs:label>belief about responsibility to act</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006304 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006304">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006302"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
