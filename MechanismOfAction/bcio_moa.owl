    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050622 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050622">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000452"/>
