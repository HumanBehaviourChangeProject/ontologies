<?xml version="1.0"?>
<rdf:RDF xmlns="http://humanbehaviourchange.org/ontology/bcio_moa.owl#"
     xml:base="http://humanbehaviourchange.org/ontology/bcio_moa.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:schema="https://schema.org/"
     xmlns:Property="https://www.wikidata.org/wiki/Property:"
     xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#">
    <owl:Ontology rdf:about="http://humanbehaviourchange.org/ontology/bcio_moa.owl">
        <owl:imports rdf:resource="http://humanbehaviourchange.org/ontology/bcio_external.owl"/>
        <owl:imports rdf:resource="http://humanbehaviourchange.org/ontology/bcio_upper_level.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://addictovocab.org/ADDICTO_0000349 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000349">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">While &apos;addiction&apos; is a disposition relating to experience motivation to use a substance, &apos;substance dependence&apos; refers to &apos;disposition related to the impaired functioning in the absence of a substance&apos;.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition towards repeated episodes of abnormally high levels of motivation to engage in a behaviour, acquired as a result of engaging in the behaviour, where the behaviour results in risk or occurrence of serious net harm.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">addiction</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000377 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000377">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A feeling involving positive affect associated with reduction in a feeling of need.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">subjective need</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">feeling of need satisfaction</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000381 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000381">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive representation of themselves by a person or group.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000399 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000399">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000381"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An identity that a person has about themselves.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000511 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000511">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling involving a strong desire or urge.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">craving</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000715 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000715">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000381"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An ongoing consideration regarding this class is whether a socially shared representation can be said to be purely cognitive.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An identity that a group holds about itself.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">group identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0001087 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0001087">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000399"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Social identity&apos; can involve a sense of one&apos;s rank or status in reference to other groups.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A self-identity in which the representation involves a relation between the identity holder and another person or group.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social identity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0001140 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0001140">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000032"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">While &apos;addiction&apos; is a disposition relating to experience motivation to use a substance, &apos;substance dependence&apos; refers to &apos;disposition related to the impaired functioning in the absence of a substance&apos;.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily disposition which is realised as impaired functioning following reduction or termination of use of a psychoactive substance.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">substance dependence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006001 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000254"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An environmental system that consists of people, their pets, social and cultural institutions and processes that influence the life of people and social groups.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social environmental system</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006002 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006002">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A social environmental system that consists of persons related as members of a domestic group, or a number of domestic groups linked through descent from a common ancestor, marriage,  adoption or other legal arrangements.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">family environmental system</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006003 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006003">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A social environmental system that consists of persons who share a bond of mutual affection and are in contact or communication.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">friendship network</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006004 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006004">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000048"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The term &apos;skill&apos; specifically refers to acquired capabilities, as behavioural theories suggested that the main difference between capabilities and skills was that skills were learned.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental capability acquired through training or practice.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006005 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000048"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Modulate refers to modifying or controlling influence on or change the strength and involves processes</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental capability that involves processes that modulate the frequency, rate or extent of a response to external or internal stimuli and that are instigated by the person themselves.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006006 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006006">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A self-regulation capability to modulate one&apos;s behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural regulation capability</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006007 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006007">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A self-regulation capability to modulate one&apos;s cognitive processes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cognitive self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006008 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006008">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A self-regulation capability to modulate one&apos;s emotional responses to a stimulus.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">emotional regulation capability</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">emotional self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006009 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006009">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050215"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behavioural capability that involves a musculoskeletal process in its realisation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">physical behavioural capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006010 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006010">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006009"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical capability acquired through training or practice.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">physical skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006011 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006011">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050215"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A personal capability includes interpersonal behaviour in its realisation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social behavioural capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006012 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006012">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006011"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A social capability acquired through training or practice.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006014 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006014">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to perceive or experience oneself as isolated from and not meaningfully involved in social groups.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social alienation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006015 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006015">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The events attended to can include internal and external events.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition that is realized by attending to events, objects or sensory patterns in experience.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">awareness</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006016 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006016">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The term plan can sometimes be used to refer to intention and sometimes for instructional plans.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to commit to enact or not enact a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural intention</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006017 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006017">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about a factor that could restrict or impede a person from engaging in a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about barriers to a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006018 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006018">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the extent to which one&apos;s own behaviour is similar to that of referent others.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about conformity to behavioural norms</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006019 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006019">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequences can be either positive or negative.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about what results from or follows the performance of a behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">response efficacy</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006020 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006020">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequences can be either positive or negative.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about consequences of behaviour in terms of their impact on the social environmental system.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about social consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006021 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006021">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequences can be either positive or negative.

Belief about consequences of goal attainment is about what types of outcomes will be achieved, rather than the value or the likelihood of these outcomes. For instance, a person may believe that studying every night will help achieve good grades. However, the value of achieving such grade may vary, and would fall under &apos;evaluative belief&apos;.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the outcomes resulting from achieving a goal.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about consequences of goal attainment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006022 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006022">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the extent to which one can direct what happens in one&apos;s future.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about control over one&apos;s future</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006023 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006023">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The perceived gain could be in anything internal or external, such as health or wealth.

The classes &apos;belief about gain&apos; and &apos;belief about reduction&apos; do not necessarily involve likelihood. Likelihoods relate to the future, However, &apos;belief about gain&apos; might be about an existing gain.  For instance, in situation where the following trade is offered: &quot;I&apos;ve given you $2, are you willing to trade it for a 50% chance of winning $6?&quot; it&apos;d be &quot;I have $2 more than I started with&quot;, there is an existing gain that is compared to a future one.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the existence of an increase from an initial reference point.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Perceived gain</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about gain</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006024 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006024">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006026"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the probability a particular behaviour will result or not result in particular outcomes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about likelihood of consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006025 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006025">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006024"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the probability of a behaviour resulting or not resulting in a change to health or wellbeing.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about likelihood of health consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006026 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006026">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequences can be either positive or negative.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the probability that a given event or state will occur or not occur in the future.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about likelihood of consequences of an occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006027 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006027">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The perceived loss could be in anything internal or external, such as wealth or health.

The classes &apos;belief about gain&apos; and &apos;belief about reduction&apos; do not necessarily involve likelihood. Likelihoods relate to the future. However, &apos;belief about reduction&apos; might be about an existing loss.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the existence of a decrease from an initial reference point.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> belief about loss</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">perceived loss</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about reduction</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006028 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006028">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006141"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief regarding whether the content of a verbal, graphic, written or recorded communication is relevant to oneself.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about message relevance</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006029 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006029">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the nature of the immediate physical and social settings in which people live, including  culture and the people and institutions with whom they interact.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about ones environment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006030 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006030">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006306"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about how serious the harm associated with an outcome could be.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about threat severity</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about severity of an outcome</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006031 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006031">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006305"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about one&apos;s personal vulnerability to a threat.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about personal susceptibility</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006033 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006033">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about consequences of behaviour in terms of how long after the performance of the behaviour they will occur.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about the timing of the consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006034 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006034">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the extent to which one is free to choose whether to perform the behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> belief about autonomy in performing behaviours</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about voluntariness of behaviour</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about choice over a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006035 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006035">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006038"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One&apos;s attributes can be evaluated as positive or negative.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An evaluative belief about one&apos;s attributes.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self-concept</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">evaluation of self</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006038 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006038">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The class &apos;evaluative belief about the consequences of behaviour&apos; can be found as the subclass of &apos;belief about consequences of behaviour&apos;.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about whether a particular aspect of the world is positive or negative.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">evaluative belief</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006039 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006039">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A person does not need to belong to a group to have a perception of their norms. For instance, the perceived norm could be in a group one aspires to belong to, and so the person might attempt to conform to it in an attempt to be accepted as a member of that group.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about what is typical for people who belong to a particular group.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about norm</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">perceived norm</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006040 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006040">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006039"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the prevalence of performance of a given behaviour by people within a group.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">perceived descriptive norm</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006041 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006041">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006039"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about whether a behaviour is appropriate and correct for people who belong to a particular group.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">perceived injunctive norm</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006042 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006042">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006039"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Key others&apos; can include &apos;people in ones social network</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about whether key others think one should perform a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">normative belief</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006043 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006043">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006154"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The phrase &apos;to organise&apos; refers to the ability to organise mental processes and sequences of actions.

&apos;self-efficacy belief for a behaviour&apos; is part of &apos;self-efficacy belief for a behaviour and its associated outcomes&apos;. However, the latter also includes a belief about the outcomes associated with behaviour.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about one&apos;s capabilities to organise and execute a behaviour and achieve the outcomes associated with this behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-efficacy belief for a behaviour and its associated outcomes</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006044 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006044">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006154"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The &apos;barrier or facilitator&apos; specified in the definition can be external or internal.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self-efficacy belief for a behaviour when a barrier or facilitator of the behaviour is encountered.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">situational self-efficacy belief for a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006045 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006045">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition that when activated, guides an interconnected network of perception, thought, emotion or behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cognitive schema</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006046 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006046">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006045"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive schema that when activated, guides a sequence of behaviours.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">action script</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">action schema</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006047 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006047">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Decision is defined as a &apos;mental disposition&apos;, a realisable entity, because a person requires a process to form, think about or reconsider a decision.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to represent one proposition as preferred from at least two.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">decision</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006048 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006048">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006047"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A decision that one behaviour is preferred from at least two behavioural options.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural decision</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006049 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006049">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive representation of an end state towards which one is striving.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">goal</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006052 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006052">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge is defined as a &apos;mental disposition&apos;, a realisable entity, because a person requires a process to access the information stored in their brain. A person knows something, when the relevant information has been retrieved from their memory. There are instances where a person might have learned something, but is unable to retrieve the information at a specific moment.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to understand the nature of the world, or a specific aspect of the world, that corresponds to the actual state of the world and is acquired through experience or learning.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">knowledge</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006053 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006053">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge regarding a particular set of actions.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">knowledge regarding a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006054 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006054">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge regarding a novel invention or way of doing something.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">knowledge regarding an innovation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006055 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006055">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge regarding a potential harm.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">knowledge regarding a threat</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006056 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006056">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006052"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge regarding a therapy or medication for a health problem.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">knowledge regarding treatment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006057 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006057">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The responses to an event can include thinking and impulses to respond.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to think or behave in a particular way in response to an internal or external event in the person&apos;s environment, which is acquired through associative learning.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">learned stimulus-reponse co-occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006058 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006058">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to evoke the representation of the sensory characteristics of objects or events when these are not immediately present to the senses.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental imagery disposition</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006059 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006059">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to act in accordance with the likely approval of others.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">motivation to comply</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">willingness to comply</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006060 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006060">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition for motivation to be guided by a focus on the presence or absence of outcomes of a certain valence.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">motivational orientation towards types of outcomes</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006061 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006061">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition for motivation to be guided by a focus on the presence or absence of positive outcomes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">prevention focused motivational orientation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006062 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006062">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition for motivation to be guided by a focus on the presence or absence of negative outcomes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">promotion focused motivational orientation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006063 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006063">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to regard certain things as fundamentally important in life, which informs standards for behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">personal value</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006064 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006064">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Psychological needs can be satisfied or thwarted.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition of a person to act to obtain or maintain a particular state due to this states importance to the persons wellbeing.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">psychological need</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006065 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006065">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A psychological need to have a sense of personal agency and initiative.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">need for autonomy</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006066 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006066">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A psychological need to have a positive evaluation of oneself and one&apos;s qualities and characteristics.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">need for self-esteem</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006067 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006067">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A psychological need to feel safe from threats.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">need for a sense of safety</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006068 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006068">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A psychological need to have caring relationships with other people.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">need for a sense of belonging and attachment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006069 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006069">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006064"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A psychological need to experience oneself as capable and effective.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">need for mastery</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">need to feel competent</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006070 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006070">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050228"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The term plan can sometimes be used to refer to intention and sometimes for instructional plans.

&apos;mental plan for a behaviour involves  plans for which details about the behaviour  are specified.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental plan that is realised in mental processes representing the steps to enact a behaviour and the conditions under which the behaviour will be enacted.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Action plan</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental plan for a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006072 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006072">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006045"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The attributes of members are not definitional. For instance, a psychologist studying psychology is not a stereotype.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive schema that when activated, guides perception, thought, emotion or behaviour toward members of a social group as having certain shared attributes, by virtue of their group membership.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stereotype</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006074 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006074">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to experience a feeling of being connected by social attachments.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social embeddedness</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006078 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006078">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An appraisal that represents an evaluation of how much one is personally obliged to respond to an event or person in need.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">appraisal of obligation to act</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006079 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006079">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Examples of &apos;reference level&apos; are the level of a behaviour and the blood glucose level.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive representation of a reference level that an individual wishes to obtain.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">desired standard</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006080 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006080">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000031"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive representation of the sensory characteristics of objects or events that are not immediately present to the senses.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental image</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006081 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006081">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A role can be assigned without being realised. A person realises a role by doing something.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A role that inheres in a human being by virtue of their social and institutional circumstances.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">personal role</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006082 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006082">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006081"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A role can be assigned without being realised. A person realises a role by doing something.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A personal role that is realised in human social processes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social role</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006085 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006085">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050308"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An intervention could be used to rearrange objects in an environment, changing the location of a person relative to these objects and thereby increasing the likelihood of a behaviour.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A spatial quality that inheres in a bearer by virtue of its position relative to other entities.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">location</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006086 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006086">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000452"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The term &apos;enables&apos; is used in the definition to specify that &apos;behavioural opportunity&apos; is a necessary condition for behaviour to occur. Although other MoAs will help bring behaviour about, there has to be a &apos;behavioural opportunity&apos; for behaviour to occur.

The term &apos;facilitates&apos; is used to specify that &apos;behavioural opportunity&apos; will make the behaviour easier. Higher behavioural opportunity will increase the likelihood of behaviour across situations and people.

The class &apos;environmental system&apos; relates to what is found in the physical and social environment , while the class &apos;behavioural opportunity&apos; is about the features of the environment and how they interact with the person .</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An environmental disposition that enables or facilitates a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006087 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006087">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006089"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">As &apos;financial behavioural opportunity&apos; will involve some physical monetary entity , it is a physical behavioural opportunity. While financial resources  have intrinsically social dimensions, the &apos;financial behavioural opportunity&apos; does not involve a social aspect.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical behavioural opportunity that involves the person having sufficient financial resources.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">financial behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006088 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006088">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006086"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The &apos;healthcare services&apos; refers to services provided for the purposes of preventing, detecting, curing or controlling a health problem or enhancing wellbeing.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behavioural opportunity regarding how easy it is for a person to approach and use a healthcare service.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">healthcare access</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006089 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006089">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006086"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A &apos;behavioural opportunity&apos; is a feature of the environmental system. While the class &apos;environmental system&apos; is about what is found in an environment, the behavioural opportunity is a feature of the environment which interacts with the person and enables or prevents a behaviour.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behavioural opportunity that involves time and parts of the environmental system that do not involve people or organisations.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">physical behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006090 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006090">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006086"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behavioural opportunity that involves the social environmental system.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006091 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006091">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006089"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical behavioural opportunity that involves the person having sufficient time or a suitable period.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">temporal behavioural opportunity</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006092 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006092">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A stimulus that arises from bodily processes and serves to elicit or guide behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bodily behavioural cue</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006093 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006093">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006092"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A stimulus that arises from mental processes and serves to elicit or guide behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental behavioural cue</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006095 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006095">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036000"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Normative behaviour can be an MoA or outcome depending on how &apos;normative behaviour&apos; is described in an intervention. The Behaviour Change-Intervention Ontology includes the class &apos;outcome behaviour&apos; , which would not be an MoA. When an intervention targets a normative behaviour to achieve an outcome behaviour, the former qualifies as an MoA.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An individual human behaviour that is commonly enacted by people that are part of a social environmental system.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">normative behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006096 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006096">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Goal pursuit process&apos; is a bodily process, as it may involve more than just mental processes .</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily process in which attempts are made to achieve a desired end state.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">goal pursuit process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006099 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006099">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036025"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Social support from social network.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An inter-personal behaviour where a person exerts an influence on the behaviour of another.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social influence behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006100 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006100">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily process by which the person experiences an internally-generated positive physical or psychological state subsequent to a response.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">internal reward for a response</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006102 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006102">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily process by which a person attempts to follow the steps in a plan.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">plan enactment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006103 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006103">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050268"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A self-regulation process that modulates the frequency, rate or extent of one&apos;s  performance of a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-regulation of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006104 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006104">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Behavioural self-regulation of behaviour can be an MoA or outcome depending on how &apos;behavioural self-regulation of behaviour&apos; is described in an intervention. The Behaviour Change-Intervention Ontology includes the class &apos;outcome behaviour&apos; , which would not be an MoA. When an intervention targets behavioural self-regulation of behaviour to achieve an outcome behaviour, the former qualifies as an MoA.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self-regulation of behaviour through the performance of a different behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural self-regulation of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006105 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006105">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self-regulation of behaviour undertaken to manage a sense of guilt, anxiety or obligation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">introjected self-regulation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006106 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006106">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self-regulation of behaviour through self-administering a reward for performing an intended behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-reinforcing self-regulation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006107 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006107">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006103"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Self-regulation undertaken to achieve consistency between one&apos;s values and the behaviour performed.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">value-congruent self-regulation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006108 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006108">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An unpleasant subjective affective feeling experienced due to perceived inconsistency between two concurrently held cognitive representations.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cognitive dissonance</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006109 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006109">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000006"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An urge is felt when an impulse is frustrated.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A subjective affective feeling that  arises when an impulse to enact a behaviour is activated but not immediately acted upon.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">feeling an urge</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006110 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006110">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000008"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive process that creates a representation of the meaning and significance of a communication, object, event or situation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">comprehension</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006111 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006111">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000008"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive process used prior to decision-making that reduces the perceived scope or complexity of available options.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">decision simplification</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006112 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006112">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000013"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Thinking that involves the generation of inferences by reasoning based on one or more statements, evaluating anticipated consequences or considering the relationships between concepts.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">reflective thinking</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006113 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006113">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process through which a perceived inconsistency between two concurrently held cognitive representations is reduced.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dissonance reduction process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006114 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006114">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that establishes a cognitive representation of the desired end state.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">goal setting process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006115 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006115">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that uses simple rules and associations learnt from experience to make judgements.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">heuristic process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006116 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006116">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000006"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The output of decision-making is a decision.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Judging during which one proposition is selected as preferred from at least two.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">decision-making</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006118 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006118">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000006"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Judging oneself or one&apos;s social group in relation to the qualities or characteristics of another person or social group.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">social comparison process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006119 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006119">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050239"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">While associative learning is often defined as learning an association between a stimulus and response, the stimulus has to be perceived. Therefore, associative learning is between perceiving or processing information about a stimulus  and one or more other mental processes.

The label &apos;associative learning&apos; is also used for a behaviour change technique  in the BCT Taxonomy v1 . The definition for this BCT reads as follows: &apos;Present a neutral stimulus jointly with a stimulus that already elicits the behavior repeatedly until the neutral stimulus elicits that behavior.&apos; As indicated by the definition, the BCT is the deliberate presentation of a stimulus, whereas the MoA is a learning process that occurs in the brain.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Learning that involves changing the strength of association between two or more mental processes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associative learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006120 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006120">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associative learning process that occurs when the pairing of a conditioned and unconditioned stimulus leads to the conditioned stimulus producing a response that would otherwise have only been produced by the unconditioned stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">classical conditioning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006121 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006121">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associative learning process in which people learn to distinguish between appropriate responses to similar but different stimuli depending on whether a generalised response is rewarded or not.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">discrimination learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006122 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006122">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An unconditioned stimulus is &quot;a stimulus that elicits a reflexive or innate response  without prior learning.&quot; . EBOOK: Psychology: The Science of Mind and Behaviour, 4e. McGraw Hill.)

A conditioned stimulus is a stimulus that, through association with an unconditioned stimulus, comes to elicit a conditioned response similar to the original unconditioned response . EBOOK: Psychology: The Science of Mind and Behaviour, 4e. McGraw Hill.)</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associative learning process in which the strength of a conditioned response is reduced because its conditioned stimulus is presented without being accompanied by the unconditioned stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">extinction in classical conditioning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006123 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006123">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Discriminative stimulus is &quot;a signal that a particular response will produce certain consequences.&quot; . EBOOK: Psychology: The Science of Mind and Behaviour, 4e. McGraw Hill.)</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associative learning process in which the rate of behavioural response in the presence of a discriminative stimulus is reduced because the response is no longer followed by a reinforcer.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">extinction in operant learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006124 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006124">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The label &apos;Generalisation of target  behavior&apos; is used for a behaviour change technique  in the BCT Taxonomy v1 . The definition for this BCT reads as follows: &apos;Advise to perform the wanted behaviour,  which is already performed in a particular situation, in another situation.&apos; Therefore, the BCT is the advice to perform the behaviour, whereas the MoA is a learning process that occurs in the brain.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associative learning process in which prior learning that an antecedent stimulus signals certain consequences of behaviour spreads to a stimulus that differs in certain aspects from the original antecedent stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">generalisation in learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006125 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006125">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associative learning process in which the likelihood of a particular behavioural response is influenced by the past consequences associated with performing that behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">operant conditioning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006126 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006126">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007613"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A memory process that deals with the receipt, storage, retrieval and modification of information regarding a set of entities associated with a particular object or event.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associative memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006127 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006127">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007613"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A memory process that deals with the receipt, storage, retrieval and modification of information regarding a personally experienced event at a particular time or place.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">episodic memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006128 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006128">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007613"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A memory process that deals with the receipt, storage, retrieval and modification of information associated with meanings, understandings or conceptual facts about the world.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">semantic memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006129 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006129">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007613"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A memory process that deals with the receipt, storage, retrieval and modification of information regarding how to perform a complex activity without conscious awareness.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">procedural memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006130 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006130">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007613"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A memory process that deals with the receipt, storage, retrieval and modification of information regarding a brief visual stimulus.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iconic memory</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006131 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006131">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process in which objects, events, people, or experiences are grouped into classes, on the basis of features shared by members of the same class and features distinguishing the members of one class from those of another.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental categorising</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006132 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006132">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The term &apos;condition&apos; may refer to the characteristics of an object, event and person.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process in which conditions are compared against a particular reference level.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mentally comparing against a standard</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006133 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006133">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that energises and directs behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006134 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006134">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006133"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Motivation that arises from emotions and impulses that result from associative learning or innate dispositions.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">automatic motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006136 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006136">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006302"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The phrase &apos;benefits to oneself&apos; includes the reduction of unpleasant internal states.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Autonomous motivation due to a behaviour&apos;s inherent enjoyment, satisfaction or benefits to oneself.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">intrinsic motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006137 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006137">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There are behaviour change techniques labelled &apos;Self-monitoring of behaviour&apos; and &apos;Self-monitoring of outcome of behaviour&apos;. These are about establishing a method for monitoring states. In contrast, the MoA is the mental process of monitoring.

&apos;Self-monitoring of behaviour&apos; is defined as &apos;Instruct self recording of specified behaviour/s  as part of a behaviour change strategy &apos; .
&apos;Self-monitoring of outcome of behaviour&apos; is defined &quot;Establish a method for the person to monitor and record their behavior as part of a behavior change strategy&quot;  .</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process in which one observes one&apos;s own behaviour or mental processes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-monitoring</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006140 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006140">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050217"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Belief about anticipated emotions&apos; is about the consequences of anything, not just behaviour. In contrast, the class &apos;belief about emotional consequences of behaviour&apos; is about the emotions resulting from behaviours.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the potential emotions experienced as result of an event.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about anticipated emotion</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006141 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006141">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief regarding aspects of a verbal, graphic, written or recorded communication.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about message</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006142 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006142">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006141"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about whether a verbal, graphic, written or recorded communication is provided by people or institutions who provide reliable information.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about the credibility of a message&apos;s source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006143 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006143">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006141"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about whether a verbal, graphic, written or recording communication provides information that can be relied on as honest or truthful.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about message trustworthiness</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006144 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006144">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief regarding whether one or more of the person&apos;s needs have been met.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about need satisfaction</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006145 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006145">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006029"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the aspects of one&apos;s immediate social settings in which people live, including culture and the people and institutions with whom they interact.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about ones social environment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006146 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006146">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006145"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about whether others will provide or are available to provide assistance or comfort to help the person cope with one or more stressors or enhance their wellbeing.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about social support</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006147 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006147">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006038"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An evaluative belief about whether a behaviour is positive or negative.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">evaluative belief about behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006148 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006148">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about consequences of behaviour in terms of whether they are positive or negative.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">evaluative belief about the consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006149 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006149">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006148"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;Desirability&apos; can range from highly desirable to highly undesirable.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An evaluative belief about the value of the consequences of behaviour to oneself.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about the personal desirability of consequences of a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006150 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006150">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006038"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An evaluative belief about the attributes of others.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">evaluative belief about others</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006152 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006152">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006154"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about one&apos;s capabilities and opportunities within an environmental system to perform a behaviour or achieve the outcomes resulting from a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about control over behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006154 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006154">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The phrase &apos;to organise&apos; refers to the ability to organise mental processes and sequences of actions.

&apos;self-efficacy belief for a behaviour&apos; is part of &apos;self-efficacy belief for a behaviour and its associated outcomes&apos;. However, the latter also includes a belief about the outcomes associated with behaviour.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about one&apos;s capabilities to organise and execute a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-efficacy belief for a behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006155 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006155">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006057"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Learned stimulus-behaviour co-occurrence can vary in strength.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to behave in a particular way in response to an internal or external event in the person&apos;s environment, which is acquired through habit learning.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Habit</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">learned stimulus-behaviour co-occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006156 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006156">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006057"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition to think in a particular way in response to an internal or external event in the person&apos;s environment, which is acquired through associative learning.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mental habit</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">learned stimulus-thought co-occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006157 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006157">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006070"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental plan regarding a strategy to deal with conditions that could prevent the performance of a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental plan for coping with barriers</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006158 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006158">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036000"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A learnt stimulus-behaviour co-occurrence can lead to activation or execution of a habitual behaviour.

Habitual behaviour can be an MoA or outcome depending on how &apos;habitual behaviour&apos; is described in an intervention. The Behaviour Change-Intervention Ontology includes the class &apos;outcome behaviour&apos; , which would not be an MoA. When an intervention targets a habitual behaviour to achieve an outcome behaviour, the former qualifies as an MoA.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behaviour that results from a learnt stimulus-behaviour co-occurrence.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">habitual behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006159 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006159">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pleasure that is experienced as a result of engaging in a behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Enjoyment of behaviour</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pleasure associated with behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006161 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006161">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000020"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental process that reduces the frequency by which an aversive cognitive representation is evoked.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">avoidance mental process</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006162 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006162">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000008"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cognitive process that involves intentionally detaching oneself from one&apos;s immediate thoughts.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">decentring</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mental distancing</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006163 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006163">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006162"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mental distancing that involves identifying particular immediate thoughts and intentionally detaching from these thoughts.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">targeted decentring</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">targeted mental distancing</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006300 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006300">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006307"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extrinsic motivation to gain or maintain social approval or a positive self-evaluation.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> introjected regulation</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">introjected motivation</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">approval-based motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006301 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006301">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006305"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about how vulnerable others are to a threat.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about others&apos; susceptibility</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006302 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006302">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006133"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Motivation that is internally generated and based on a behaviour being interesting or valuable to an individual.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">autonomous motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006303 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006303">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about whether oneself or another person is obligated to take a certain course of action.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about responsibility to act</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006304 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006304">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006302"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Autonomous motivation due to the perceived value or importance of the behaviour to oneself.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> identified regulation</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">identified motivation</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural importance-based motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006305 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006305">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006306"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about vulnerability to a threat.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about susceptibility to a risk</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about susceptibility to a threat</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006306 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006306">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about a potential harm.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about threat</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006307 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006307">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006133"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Motivation that is based on external rewards, punishments or pressure regarding a behaviour.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">controlled motivation</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">extrinsic motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006308 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006308">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006302"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Autonomous motivation due to a behaviour being perceived to satisfy a core psychological need.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> integrated regulation</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">integrated motivation</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fundamental need-based motivation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_006309 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_006309">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006119"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associative learning process that involves strengthening an association between a stimulus and an impulse through repetition of co-occurrence of the stimulus and the behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">habit learning</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_015430 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_015430">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006081"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A role can be assigned without being realised. A person realises a role by doing something.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A role that is realised in a person by doing a specified type of work or working in a specified way.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">occupational role</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_036000 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_036000">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A bodily process of a human that involves co-ordinated contraction of striated muscles controlled by the brain.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">individual human behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_036025 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_036025">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An individual human behaviour that involves an interaction between two or more people.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">inter-personal behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_036034 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_036034">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036025"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An inter-personal behaviour that involves the intentional transmission of information.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">communication behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050213 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050213">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A social environmental system that consists of persons who have shared interests relating to their occupational roles and are in contact or communication.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">professional network</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050214 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050214">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A self-regulation capability to modulate ones attention toward internal or external stimuli.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">attentional self-regulation capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050215 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050215">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000043"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A personal capability that includes behaviours in its realisation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behavioural capability</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050216 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050216">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006140"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the potential regret experienced as a result of an event.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about anticipated regret</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050217 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050217">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000041"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequences can be either positive or negative.</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the outcomes resulting from an occurrence.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about consequences of an occurrence</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050218 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050218">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about consequences of behaviour in terms of emotions.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about emotional consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050219 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050219">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006019"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about consequences of behaviour in terms of health and wellbeing.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about health consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050220 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050220">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006024"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the probability of a behaviour resulting or not resulting in a change to emotions.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about likelihood of emotional consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050221 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050221">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006024"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief about the probability of a behaviour resulting or not resulting in a change to social outcomes.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belief about likelihood of social consequences of behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050222 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050222">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A self-regulation capability that is acquired through training or practice.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">self-regulatory skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050223 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050223">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000075"/>
