        <rdfs:label>socially-related behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050457 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050457">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_036000"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An &lt;individual human behaviour&gt; that conveys a thought or feeling.</obo:IAO_0000115>
        <rdfs:label>expressive behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050560 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050560">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006049"/>
