    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_050623 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_050623">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_006024"/>
