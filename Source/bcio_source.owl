        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A personal care worker that provides care and supervision for children in non-domestic settings.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 5311</rdfs:isDefinedBy>
        <rdfs:label>child care worker</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010073 -->

