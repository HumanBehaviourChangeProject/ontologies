        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010044"/>
        <obo:IAO_0000112>Community development worker, Community services worker, Crisis intervention worker, Disability services worker, Family services worker, Life skills instructor, Mental health support worker, Welfare support worker, Womenâ€™s shelter supervisor, Youth services worker</obo:IAO_0000112>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A technician and associate professional that implements social assistance programmes and community services and assist clients to deal with personal and social problems.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 3412</rdfs:isDefinedBy>
        <rdfs:label>social work associate professional</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010060 -->

