        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A technician and associate professional that provides support to ministers of religion or to a religious community, undertake religious works, preach and propagate the teachings of a particular religion and endeavour to improve well-being through the power of faith and spiritual advice.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 3413</rdfs:isDefinedBy>
        <rdfs:label>religious associate professional</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010061 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010061">
