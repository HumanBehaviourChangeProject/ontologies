        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A services and sales worker that provides care, supervision and assistance for children, patients and elderly, convalescent or disabled persons in institutional and residential settings.</obo:IAO_0000115>
        <obo:IAO_0000119>International Standard Classification of Occupations (ISCO)</obo:IAO_0000119>
        <rdfs:isDefinedBy>ISCO: 53</rdfs:isDefinedBy>
        <rdfs:label>personal care worker</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010072 -->

