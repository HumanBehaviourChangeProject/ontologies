        <rdfs:label>formal communication style</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044105 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_044105">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_044004"/>
