        <rdfs:label>structured communication style</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044121 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_044121">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_044004"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
