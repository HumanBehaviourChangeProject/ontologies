        <rdfs:label>relationship building</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044125 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_044125">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_044003"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An attribute of an oral communication relating to the rhythm and pitch of what is said.</obo:IAO_0000115>
        <rdfs:label>tone of voice</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044126 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_044126">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_044003"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>An attribute of an oral communication relating to the speed with which an individual speaks.</obo:IAO_0000115>
        <rdfs:label>oral communication pace</rdfs:label>
    </owl:Class>
    


