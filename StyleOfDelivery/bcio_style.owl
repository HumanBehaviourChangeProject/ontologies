        <obo:IAO_0000118>sensitivity, compassion</obo:IAO_0000118>
        <rdfs:label>empathic communication style</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044102 -->

