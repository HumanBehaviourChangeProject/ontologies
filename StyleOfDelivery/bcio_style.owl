    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044123 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_044123">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_044004"/>
        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A communication style which demonstrates human interest and caring.</obo:IAO_0000115>
        <obo:IAO_0000118>friendly communication style</obo:IAO_0000118>
        <rdfs:label>warm communication style</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044124 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_044124">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_050384"/>
