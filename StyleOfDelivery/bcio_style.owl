        <obo:IAO_0000114>Published</obo:IAO_0000114>
        <obo:IAO_0000115>A communication style characterised by being responsive to the recipient&apos;s activity.</obo:IAO_0000115>
        <rdfs:label>interactive communication style</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_044109 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_044109">
