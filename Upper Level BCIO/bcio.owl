        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A family member that is a family relationship between two persons with at least one shared parent.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sibling relationship</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010099 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010099">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010095"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">daughter, son, step-daughter, step-son</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A family member that is an offspring relationship from a person to their parent.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">child relationship</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010100 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010100">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010094"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">carer</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relatedness between person source and the target population that is an individual who cares, unpaid, for a friend or family member who, due to illness or disability, requires support in their daily life activities.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">carer</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010101 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010101">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010094"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relatedness between person source and the target population that is a person whom the participant knows, likes and trusts, typically exclusive of sexual or family relations.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">friend</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010102 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010102">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010094"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relatedness between person source and the target population that is a person with whom the participant works in a profession or business.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">colleague</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010103 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010103">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010094"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relatedness between person source and the target population that is a person which hires the services of the participant.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">employer</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010104 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010104">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010094"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relatedness between person source and the target population that is descried as matched to intervention recipients on the basis of peerness  age, social status, gender, shared experience, shared health status etc.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">peer</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010105 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010105">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010094"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relatedness between person source and the target population that is a source who is known and working to deliver intervention in own community.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">embedded in participants community</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010106 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010106">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000027"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010000"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A count data item that is the number of providers that an individual participant in the intervention encounters.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">number of people delivering intervention to each participant</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010107 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010107">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000027"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010000"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A count data item that is the total number of providers that are available and able to deliver the intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">total number of people able to deliver intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010108 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010108">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000052"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010002"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A social or demographic characteristic of a human being who is the bearer of a person source role.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">socio demographic attribute of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010109 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010109">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that is a time quality inhering in a bearer by virtue of how long the bearer has existed.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">age of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010110 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010110">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">gender</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that is an individual&apos;s perception of having a particular gender, which may or may not correspond with their birth sex.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">gender of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010111 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010111">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010110"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">female, woman</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A gender of person source that is the cultural gender role of female.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">female gender</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010112 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010112">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010110"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">male, man</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A gender of person source that is the cultural gender role of male.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">male gender</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010113 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010113">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010110"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">non-binary, transexual</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A gender of person source that reports not belonging to the cultural gender role distinctions of either male or female.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">other gender</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010114 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010114">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">black, white, Indian, Chinese, Somalian</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that is the ethnic group to which an individual identifies as belonging, where an ethic group is a population whose members have a common heritage that is real or presumed such as common culture, language, religion, behaviour or biological trait.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ethnic group membership of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010115 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010115">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Muslim, Christian, Hindu, Sikh, Buddhist, Judaism</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that is a religious group to which an individual identifies as belonging, where a religious group is a group of people characterised by the practice of a common religion.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">religious group membership of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010116 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010116">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fluent, native</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that is an individuals ability to speak or perform in the intervention language.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">language proficiency of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010117 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010117">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">diabetes, cancer, depression, obesity, overweight</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that represents their mental or physical condition.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">health status of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010118 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010118">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">source is highly physically active</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that is their amount or experience of the interventions target behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">target behaviour of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010119 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010119">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010108"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A socio-demographic attribute of person source that is their existing psychological attributes related to or potentially affecting the target behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">psychological influence on intervention delivery of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010120 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010120">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000052"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010002"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A disposition that is an expert skill or knowledge held by the source delivering the behaviour change intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">expertise of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010121 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010121">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010120"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">trained</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An expertise of person source that is knowledge or skills held in order to deliver the behaviour change intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">knowledge or skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010122 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010122">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010121"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bachelors degree, certification, accredited, qualified</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A knowledge or skill that is already possessed by the source which allows them to deliver the behaviour change intervention, including educational level and qualifications.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pre-existing knowledge or skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010123 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010123">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The division of expertise into different disciplines or fields is complex and depends on historical, social and structural factors.</obo:IAO_0000112>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">psychology, yoga, cognitive behavioural therapy, hypnotism</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An attribute that is a field of knowledge or practice.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">expertise discipline</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010124 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010124">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010123"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000052"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010122"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">psychology, yoga, cognitive behavioural therapy, hypnotism</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The expertise discipline in which the person source has acquired their pre-existing knowledge and skills.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">discipline of pre-existing knowledge or skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010125 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010125">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010121"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A knowledge or skill that is additional knowledge or skills supplied to the source to allow them to deliver the behaviour change intervention, including educational level and qualifications.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">acquired knowledge or skill</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010126 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010126">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000027"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010121"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">full-day, 6 years</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A knowledge or skill that is the duration of experience in related domain held by person source.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">amount of experience</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010127 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010127">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010122"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">member of British Psychological Society</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A pre-existing knowledge or skill that is recognised through affiliation to a formal group or organisation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">affiliation to a formal group or organisation</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010128 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010128">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000057"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010002"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">supervised</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A process in which a person source is formally provided, by an individual with appropriate expertise, with corrective and skill-enhancing feedback, regarding the person sources performance in delivering the intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">supervision of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010129 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010129">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000057"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010002"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">voluntary basis, volunteering</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A process in which a person source delivers the intervention on a voluntary basis without formal compensation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">volunteering of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010130 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010130">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000057"/>
                <owl:someValuesFrom rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010002"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A process in which a person source is paid or compensated for delivering the intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">payment of person source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010131 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010131">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010130"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">paid in cash, vouchers</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A payment of person source that is money, vouchers or valued objects given to the source for delivering the intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">monetary payment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010132 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010132">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010130"/>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">course credit</obo:IAO_0000112>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A payment of person source that is non-monetary compensation given to the source for delivering the intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">non-monetary payment</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010133 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010133">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A BCI source whose occupational or voluntary role is focused on delivery of the behaviour change intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">source role related to intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010134 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010134">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A BCI source that is involved in the development of intervention content.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">source involved in development of intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010135 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010135">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010134"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A source involved in development of intervention that has developed the intervention content in collaboration with key stakeholders such as patients or community members.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">source involved in co-production of intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010138 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010138">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_010000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A BCI source role that is borne by an organisation.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">organisational source role</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_010139 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_010139">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000245"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An organisation that is the bearer of a behaviour change intervention source role, such as voluntary, public or commercial organisations delivering a behaviour change intervention.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">organisation source</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011000 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011000">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_043000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An attribute of a BCI delivery that is the physical or informational medium through which a BCI is provided.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">BCI mode of delivery</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">behaviour change intervention mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011001 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011001">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves intentional transmission of a representation of the world to an intervention recipient with the aim of changing that person&apos;s representation of the world.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This includes delivery of rewards, prompts, and cues that result in learning and information about the environment and environmental contingencies.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">informational mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011002 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011002">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informational mode of delivery that involves a person as intervention source who interacts with an intervention recipient in real time.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">human interactional mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011003 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011003">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Human interactional mode of delivery that involves an intervention source and recipient being together in the same location and communicating directly.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">face to face mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011004 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011004">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Human interactional mode of delivery that involves an intervention source and recipient being in different locations and communicating through a communication channel.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">at-a-distance mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011005 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011005">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informational mode of delivery that involves use of printed material.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Can include paper, acetate, text, diagrams and photographic images.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">printed material mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011006 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011006">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Printed material mode of delivery that involves a letter or postcard that can be sent through the post or handed directly to the recipient</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">letter mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011007 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011007">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Printed material mode of delivery that involves display of a poster, sign or notice in a public location.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">public notice mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011008 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011008">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Printed material mode of delivery that involves use of of a printed publication.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes leaflets, brochures, newspapers, newsletter, booklets, mazazines, manuals or worksheets.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">printed publication mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011009 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011009">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011005"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Printed material mode of delivery that involves information printed on a product or its packaging, or a label attached to or included with, a product or its packaging, and aims to convey information about that product.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">labelling mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011010 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011010">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informational mode of delivery that involves electronic technology in the presentation of information to an intervention recipient.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic has been used rather than digital because not all of the content may involve digital encoding - some may be analogue. Also, it is the electronic nature that is central not the means of encoding.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">electronic mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011011 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011011">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of information that is broadcast and displayed by television.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes internet and satellite television.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">television mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011012 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011012">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of information by a handheld mobile digital device that can store, retrieve and process data.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mobile digital device mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011013 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011013">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of information by a desktop or laptop computer.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">computer mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011014 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011014">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of information by an electronic screen positioned in a public location.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">electronic billboard mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011015 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011015">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of information by an electronic device that is worn or carried on the body.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes a watch, clip-on device, spectacles, in-ear devfice, vibrating device.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">wearable electronic device mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011016 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011016">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves an electronic device positioned in the environment of the intervention recipient that can gather information and respond to commands.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes robots, and &apos;internet of things&apos;.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">electronic environmental object mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011017 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011017">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of a 3-D image.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes hologram but does not include virtual reality headsets.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">3-D projection mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011018 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011018">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves use of virtual reality through a virtual reality headset and optionally body movement sensors.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">virtual reality mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011019 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011019">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of information stored on an object that is inserted into a playing device.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes cassettes, video tapes, DVDs, CDs.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">playable electronic storage mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011020 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011020">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves presentation of audio information that is broadcast and received by a radio receiver.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes digital radio and audio that is streamed as a podcast.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">radio broadcast mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011021 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011021">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves a communication process in which a signal is sent by a caller to a recipient to alert them of the communication intent, giving the recipient the opportunity to enage with the communication.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Added in order to be able to capture the concept of a &apos;call&apos;.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">call mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011022 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011022">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011021"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Call mode of delivery that involves only audio information in the communication.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes automated calls and audio messaging.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">audio call mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011023 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011023">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011021"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Call mode of delivery that involves video and audio information in the communication.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">video call mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011024 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011024">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011021"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Call mode of delivery that involves textual information in the communication.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Text message can include emojis, and additional audio and pictorial material. Includes SMS, WhatsApp and other messaging services.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">messaging mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011025 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011025">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves communication by email.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">email mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011026 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011026">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves the intervention recipient playing a computer game.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">video game mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011027 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011027">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves the intervention recipient interacting with a website.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">website mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011028 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011028">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves the intervention recipient interacting with a mobile application.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The distinction between websites and mobile applications has become blurred with the advent of webapps.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mobile application mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011029 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011029">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011010"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Electronic mode of delivery that involves the intervention recipient being given access to an e-book.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">e-book mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011030 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011030">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informational mode of delivery that involves sound.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">audio informational mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011031 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011031">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informational mode of delivery that involves visual images.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">visual informational mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011032 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011032">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informational mode of delivery that involves written text.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">textual mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011033 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011033">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves changing the physical shape, size, structure or appearance of objects in the environment of the intervention recipient.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is a very heterogeneous class that will need to be further subdivided.</obo:IAO_0000232>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This does not include use of textual or pictorial information. It includes lighting, speed humps, use of music, shape and size of containers of consumables.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">environmental change mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011034 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011034">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves devices or substances that alter bodily processes or structure.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Is intended to be disjoint from informational mode of delivery.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">somatic mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011035 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011035">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011034"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Somatic mode of delivery that involves ingestion of a chemical into the body.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ingestion mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011036 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011036">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ingestion mode of delivery that involves ingestion of a chemical through the skin.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">transdermal mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011037 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011037">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Somatic mode of delivery that involves ingestion of a chemical through the stomach or intestine.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">alimentary mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011038 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011038">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011037"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alimentary mode of delivery that involves swallowing of a pill or oral capsule.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pill mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011039 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011039">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011037"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alimentary mode of delivery that involves swallowing of a liquid.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ingestable liquid mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011040 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011040">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ingestion mode of delivery that involves absorption of a chemical through the lining of the buccal cavity.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">buccal mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011041 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011041">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ingestion mode of delivery that involves absorption of a chemical through  the upper airways or lungs by inspiration.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">inhalation mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011042 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011042">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ingestion mode of delivery that involves a chemical being introduced into body tissue through a hollow needle that punctures the skin.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">injection mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011043 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011043">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011042"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Injection mode of delivery in which the tissue receiving the chemical is subcutaneous tissue.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">subcutaneous injection mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011044 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011044">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011042"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Injection mode of delivery in which the tissue receiving the chemical is venous blood.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">intravenous injection mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011045 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011045">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011042"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Injection mode of delivery in which the tissue receiving the chemical is muscle.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">intramuscular injection mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011046 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011046">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011035"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ingestion mode of delivery that involves a device that is worn on the body.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes insulin pump.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">wearable ingestion mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011047 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011047">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011034"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ingestion mode of delivery that involves chewing of a soft material.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This includes chewing gum. often involves ingestion of a chemical that is released by chewing and absorbed through the lining of the buccal cavity, but this is not necessary.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">chewable substance mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011048 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011048">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011034"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mode of delivery that involves application of a physical stimulus to the body.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">physical stimulus mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011049 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011049">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011048"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Physical stimulus mode of delivery that involves exposure of light to the body</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes photo therapy</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">light exposure mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011050 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011050">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011048"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Physical stimulius mode of delivery that involves application of heat or cold to the body.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">temperature mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011051 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011051">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011048"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Physical stimulus mode of delivery that involves application of electrical stimulation to the body.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">electrical stimulation mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011052 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011052">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011048"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Physical stimulus mode of delivery that involves application of physical pressure to the outside of the body.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes massage.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">physical pressure mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011053 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011053">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011048"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Physical stimulus mode of delivery that involves a device that is worn on the body.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">wearable stimulus mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011054 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011054">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of of delivery that involves modifying the structure of the body of the recipient of the intervention.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Includes surgery.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">somatic alteration mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011055 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011055">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <owl:disjointWith rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011056"/>
        <owl:disjointWith rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011057"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves one recipient in the location where the intervention is delivered.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">individual-based mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011056 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011056">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <owl:disjointWith rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011057"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves two recipients in the location where the intervention is delivered who have an interpersonal relationship.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pair-based mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011057 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011057">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves three or more people in the location where the intervention is delivered.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">group-based mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011058 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011058">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <owl:disjointWith rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011059"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery in which the only causal influence is from the intervention source to the recipient.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">uni-directional mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011059 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011059">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery in which there is causal influence from the intervention source to the recipient and from the recipient to the source.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">interactional mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011060 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011060">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <owl:disjointWith rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011061"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves delivery and receipt of the intervention or its components occurring at the same time or very close in time.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">synchronous mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011061 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011061">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that involves receipt of the intervention or its components taking place a significant period of time after delivery.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">asynchronous mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011062 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011062">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <owl:disjointWith rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011063"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that is not dependent on actions on the part of the intervention recipient.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">push mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011063 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011063">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mode of delivery that requires some action on the part of the recipient.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pull mode of delivery</rdfs:label>
    </owl:Class>
    


    <!-- http://humanbehaviourchange.org/ontology/BCIO_011064 -->

    <owl:Class rdf:about="http://humanbehaviourchange.org/ontology/BCIO_011064">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_011000"/>
